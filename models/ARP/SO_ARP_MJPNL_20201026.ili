    CLASS Vereinbarung_Bewirtschafter_Historie =
      !!@ ili2db.dispName = "Vereinbarungs-NR MJPNL"
      Vereinbarungs_Nr : MANDATORY TEXT*50;
      GELAN_PID_GELAN : MANDATORY 200000 .. 9999999;
      /** Jahr des neu zugewiesenen GELAN Bewirtschafters
       */
      !!@ ili2db.dispName = "Jahr des neuen Bewirtschafters"
      Jahr_Bewirtschafterwechsel : MANDATORY 1900 .. 2100;
    END Vereinbarung_Bewirtschafter_Historie;

    DOMAIN

      /** MJPNL Programmtyp oder Attribut Flächenart
       */
      Vereinbarungsart = (
        !!@ ili2db.dispName = "Ackerlebensraum (Saum)"
        ALR_Saum,
        !!@ ili2db.dispName = "Ackerlebensraum (Buntbrache)"
        ALR_Buntbrache,
        !!@ ili2db.dispName = "Wasserbeeinflusste Lebensräume Weiden"
        WBL_Weide,
        !!@ ili2db.dispName = "Wasserbeeinflusste Lebensräume Wiesen"
