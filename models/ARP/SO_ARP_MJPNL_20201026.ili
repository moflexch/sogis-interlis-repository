    /** Es gibt nur einen Basisvertrag pro GELAN Bewirtschafter.
     */
    !!@ ili2db.dispName = "Basisvertrag für GELAN Person"
    CLASS Basisvertrag
    EXTENDS Dokument =
      !!@ ili2db.dispName = "Personen ID GELAN"
      GELAN_PID_GELAN : MANDATORY TEXT*20;
      Datum_Vertrag : MANDATORY FORMAT INTERLIS.XMLDate "1900-1-1" .. "2100-12-31";
    END Basisvertrag;

    /** Naturschützerische Zusatzleistungen ergänzend zum Basisvertrag des Ackerlebensraumes Buntbrache.
     */
    !!@ ili2db.dispName = "Beurteilung ALR Buntbrache"
    CLASS Beurteilung_ALR_Buntbrache
    EXTENDS Beurteilung =
      /** Betrieb liegt in der Regel innerhalb des Massnahmengebiet BFF zur Förderung von Feldhasen und Feldlerchen.
       */
      !!@ ili2db.dispName = "Massnahmengebiet Förderung von Feldhasen und Feldlerchen"
      Einstiegskriterium_Massnahmengebiet_FeldhasenLerchen : MANDATORY BOOLEAN;
      /** Betrieb verfügt über mind. 1.5 ha offene Ackerfläche in FFF.
       */
      !!@ ili2db.dispName = "Mind. 1.5 ha offene Ackerfläche in FFF."
      Einstiegskriterium_min_Ackerflaeche : MANDATORY BOOLEAN;
      /** Nicht längsseitig entlang von Flurwegen. In der Regel mind. 30 Meter von Wegen mit Hartbelag entfernt.
       */
      !!@ ili2db.dispName = "Nicht längsseitig entlang von Flurwegen."
      Einstiegskriterium_nicht_entlang_Flurwegen : MANDATORY BOOLEAN;
      /** Bevorzugt vor Ansaat Acker (keine Kunstwiese)
       */
      !!@ ili2db.dispName = "vor Ansaat Acker"
      Einstiegskriterium_vor_Ansaat_Acker : MANDATORY BOOLEAN;
      /** Standort nicht im Waldschatten, entlang von Hecken, Obstanlagen oder im Bereich von Gewässern.
       */
      !!@ ili2db.dispName = "Standort nicht im Waldschatten, entl. v. Hecken, Obstanlagen o. im Gewässerbereich."
      Einstiegskriterium_Standort_diverse_Kriterien : MANDATORY BOOLEAN;
      /** Maximale Breite von 12 Meter
       */
      !!@ ili2db.dispName = "Max. Breite 12 Meter"
      Einstiegskriterium_MaxBreite_12m : MANDATORY BOOLEAN;
      /** Mindestens 8 Jahre. Option für Verlängerung bei vorhandenen Arten der Faunaliste bis max. 12 Jahre. Anschliessend Verlängerung um jeweils ein Jahr in Absprache mit dem Kanton möglich.
       */
      !!@ ili2db.dispName = "Mind. 8 Jahre Artenschutz, Option für Verlängerung"
      Einstiegskriterium_Mindestdauer_Artenschutz_Fauna : MANDATORY BOOLEAN;
      /** Der Bewirtschafter verpflichtet sich die Fläche regelmässig auf Problempflanzen zu kontrollieren und fachgerecht (nach DZV) zu bekämpfen. Das ARP berät und ermahnt den Bewirtschafter vor Erreichen der Bekämpfungsschwelle der Problempflanzen zu geeigneten Massnahmen.
       */
      !!@ ili2db.dispName = "Verpflichtung Problempflanzen zu bekämpfen"
      Einstiegskriterium_Problempflanzen_bekaempfen : MANDATORY BOOLEAN;
      /** Zwischenbeurteilung gemeinsam alle 2 Jahre
       */
      !!@ ili2db.dispName = "Zwischenbeurteilung gemeinsam alle 2 Jahre"
      Einstiegskriterium_Zwischenbeurteilung_alle_2Jahre : MANDATORY BOOLEAN;
      /** Mithilfe beim Monitoring von Qualitätsindikatorarten durch Bewirtschafter.
       */
      !!@ ili2db.dispName = "Monitoring v. Qualitätsindikatorarten durch Bewirtschafter"
      Einstiegskriterium_Monitoring_durch_Bewirtschafter : MANDATORY BOOLEAN;
      /** Art 1 gemäss Faunaliste der Subregion
       */
      !!@ ili2db.dispName = "Fauna Artenförderung 1"
      Faunabonus_Art_1 : TEXT*255;
      /** Art 2 gemäss Faunaliste der Subregion
       */
      !!@ ili2db.dispName = "Fauna Artenförderung 2"
      Faunabonus_Art_2 : TEXT*255;
      /** Art 3 gemäss Faunaliste der Subregion
       */
      !!@ ili2db.dispName = "Fauna Artenförderung 3"
      Faunabonus_Art_3 : TEXT*255;
      /** Faunabonus Anzahl Arten.
       */
      !!@ ili2db.dispName = "Artenvielfalt - Anzahl Arten"
      Faunabonus_Anzahl_Arten : MANDATORY 0 .. 3;
      /** Artenvielfalt total (max. CHF 300.- pauschal). Pro Art 100.- CHF.
       */
      !!@ ili2db.dispName = "Artenvielfalt total (max. CHF 300.- pauschal)"
      Faunabonus_Artenvielfalt_Abgeltung_pauschal : MANDATORY 0.00 .. 300.00 [Units.CHF];
      /** Fachgerechte Ansaat und Pflege (vgl. Merkblatt agridea)
       */
      !!@ ili2db.dispName = "Fachgerechte Ansaat und Pflege"
      Bewirtschaftung_Ansaat_und_pflege : MANDATORY BOOLEAN;
