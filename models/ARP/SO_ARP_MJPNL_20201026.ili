        !!@ ili2db.dispName = "Kat. II/RF (Habermarkwiese)"
        Kat_II_RF,
        /** Trespenwiese (trocken); Pfeifengraswiese / Hochstaudenried (feucht)
         */
        !!@ ili2db.dispName = "Kat. II (artenreiche Wiese)"
        Kat_II_artenreicheWiese,
        /** Trespenwiese (trocken); Kleinseggenried mit Wollgras (feucht)
         */
        !!@ ili2db.dispName = "Kat. I (besonders artenreiche Wiese)"
        Kat_I_besondersartenreicheWiese
      );

      Wiesenstruktur = (
        !!@ ili2db.dispName = "2-schichtig"
        zwei_schichtig,
        !!@ ili2db.dispName = "1-schichtig"
        ein_schichtig,
        dicht,
        locker,
        !!@ ili2db.dispName = "regelmässig"
        regelmaessig,
        !!@ ili2db.dispName = "unregelmässig"
        unregelmaessig
      );
    STRUCTURE Bodeneigenschaft_ = value : MANDATORY Bodeneigenschaft; END Bodeneigenschaft_;
    STRUCTURE Topografische_Eigenschaften_ = value : MANDATORY Topografische_Eigenschaften; END Topografische_Eigenschaften_;
    STRUCTURE Weide_BesondereStrukturen_ = value : MANDATORY Weide_BesondereStrukturen; END Weide_BesondereStrukturen_;
    STRUCTURE Weidnarbe_ = value : MANDATORY Weidnarbe; END Weidnarbe_;
    STRUCTURE Wiesenstruktur_ = value : MANDATORY Wiesenstruktur; END Wiesenstruktur_;

    CLASS Berater =
      Name : MANDATORY TEXT*255;
      Vorname : MANDATORY TEXT*255;
      !!@ ili2db.dispName = "Amt oder Organisation oder Ort"
      Amt_Organisation_Ort : MANDATORY TEXT*255;
      Email : TEXT*255;
      Telefon : TEXT*30;
      MobilTelefon : TEXT*30;
      /** Zuständigkeiten (Themen und Regionen (Gemeinden/Bezirke))
       */
      !!@ ili2db.dispName = "Zuständig für (Regionen und Themen)"
      ZustaendigFuer : MTEXT*2000;
    END Berater;

    CLASS Beurteilung (ABSTRACT) =
      Beurteilungsdatum : MANDATORY INTERLIS.XMLDateTime;
      /** Wird von ARP-internen Mitarbeitern freigegeben. Rechtevergabe beachten.
       */
      Beurteilungsfreigabe : MANDATORY BOOLEAN;
      Bemerkungen : MTEXT*2000;
      Kopie_an_Bewirtschafter : BOOLEAN;
      !!@ ili2db.dispName = "Generische Abgeltung - Beschrieb der Leistung"
      Abgeltung_generisch_Text : TEXT*255;
      /** Dieser Betrag ist pauschal und kann positiv oder negativ sein (Abschlagszahlung für zuviel bezahlte Subventionen).
       */
      !!@ ili2db.dispName = "Abgeltung generisch Pauschalbetrag"
      Abgeltung_generisch_Betrag : -10000.00 .. 10000.00 [Units.CHF];
    END Beurteilung;

    /** Damit mehrere BFS-Nr pro Attribut abgespeichert werden können
     */
    STRUCTURE BFS_Nr =
      !!@ ili2db.dispName = "BFS-Nr"
      BFS_Nr : MANDATORY CHAdminCodes_V1.CHMunicipalityCode;
    END BFS_Nr;

    CLASS Dokument (ABSTRACT) =
      /** Pfad im Dateisystem oder URL. Relativ zu Basispfad.
       */
      Dateipfad_oder_URL : MANDATORY TEXT*1000;
    END Dokument;

    STRUCTURE String_List =
      String : MANDATORY TEXT*255;
    END String_List;

    /** Umweltziele Landwirtschaft. Quelle: Bafu/Agroscope.
     * 
     * https://www.agroscope.admin.ch/agroscope/de/home/themen/umwelt-ressourcen/biodiversitaet-landschaft/oekologischer-ausgleich/umweltziele-landwirtschaft.html
     * 
     * Daten müssen beim Import vom LV03 ins LV95 konvertiert werden.
     */
    !!@ ili2db.dispName = "UZL Subregion"
    CLASS UZL_Subregion =
      SRCode : MANDATORY TEXT*3;
      SRName : MANDATORY TEXT*255;
      Geometrie : MANDATORY GeometryCHLV95_V1.MultiSurface;
    END UZL_Subregion;

    /** Hier wird der Bewirtschafterwechsel dokumentiert
     */
