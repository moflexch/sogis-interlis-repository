      AbrechnungPerVereinbarung -<#> {1} Abrechnung_per_Vereinbarung;
    END AbrechnungLeistung_AbrechnungPerVereinbarung;

    ASSOCIATION AbrechnungPerVereinbarung_AbrechnungPerBewirtschafter =
      AbrechnungPerVereinbarung -- {1..*} Abrechnung_per_Vereinbarung;
      AbrechnungPerBewirtschafter -<#> {1} Abrechnung_per_Bewirtschafter;
    END AbrechnungPerVereinbarung_AbrechnungPerBewirtschafter;

    ASSOCIATION Basisvertrag_Berater =
      Basisvertrag -- {0..*} Basisvertrag;
      Berater -- {1} Berater;
    END Basisvertrag_Berater;

    ASSOCIATION Beurteilung_Faunaliste =
      Beurteilung -- {0..*} Beurteilung;
      Art_Fauna -- {0..*} Faunaliste;
      /** optional for now, maybe later mandatory
       */
      !!@ ili2db.dispName = "Häufigkeit (quantitativ)"
      Haeufigkeit_quantitativ : 1 .. 1000;
    END Beurteilung_Faunaliste;

    ASSOCIATION Beurteilung_Floraliste =
      Beurteilung -- {0..*} Beurteilung;
      Art_Flora -- {0..*} Floraliste;
      !!@ ili2db.dispName = "Häufigkeit (qualitativ)"
      Haeufigkeit_qualitativ : Floraliste_Haeufigkeit;
      !!@ ili2db.dispName = "Häufigkeit (quantitativ)"
      Haeufigkeit_quantitativ : 0 .. 10000;
    END Beurteilung_Floraliste;

    ASSOCIATION Foto_Beurteilung =
      Foto -- {0..*} Foto;
      Beurteilung -<#> {1} Beurteilung;
    END Foto_Beurteilung;

    /** Zusammenzug von Fauna und Floraliste (für Massnahmen Artenförderung)
     */
    !!@ ili2db.dispName = "Fauna- und Floraliste (kombiniert)"
    VIEW Fauna_Flora_Liste
    UNION OF
      Fa ~ Faunaliste,
      Fl ~ Floraliste;
    =
      Name_Lateinisch: MANDATORY TEXT*255 := Fa -> Name_Lateinisch,
                                             Fl -> Name_Lateinisch;
      Name_Deutsch: MANDATORY TEXT*255 := Fa -> Name_Deutsch,
                                          Fl -> Name_Deutsch;
      FF_Art: MANDATORY TEXT*50;
    END Fauna_Flora_Liste;

    ASSOCIATION AbrechnungLeistung_Vereinbarung =
      Leistungsabrechnung -- {0..*} Abrechnung_per_Leistung;
      Vereinbarung -<#> {1} Vereinbarung;
    END AbrechnungLeistung_Vereinbarung;

    ASSOCIATION Beurteilung_Vereinbarung =
      Beurteilung -- {0..*} Beurteilung;
      Vereinbarung -- {1} Vereinbarung;
    END Beurteilung_Vereinbarung;

    ASSOCIATION Strukturelement_Flaeche_Vereinbarung =
      Strukturelement_Flaeche -- {0..*} Strukturelement_Flaeche;
      Vereinbarung -<#> {1} Vereinbarung;
    END Strukturelement_Flaeche_Vereinbarung;

    ASSOCIATION Strukturelement_Linie_Vereinbarung =
      Strukturelement_Linie -- {0..*} Strukturelement_Linie;
      Vereinbarung -<#> {1} Vereinbarung;
    END Strukturelement_Linie_Vereinbarung;

    ASSOCIATION Strukturelement_Punkt_Vereinbarung =
      Strukturelement_Punkt -- {0..*} Strukturelement_Punkt;
      Vereinbarung -<#> {1} Vereinbarung;
    END Strukturelement_Punkt_Vereinbarung;

    ASSOCIATION VB_Bewirtschafter_Historie =
      Vereinbarung -<#> {1} Vereinbarung;
      Bewirtschafter_Historie -- {0..*} Vereinbarung_Bewirtschafter_Historie;
    END VB_Bewirtschafter_Historie;

    /** Pro Vereinbarung gibt es nur 1 Subregion. Wenn 2 oder mehr Subregionen betroffen sind, wird die Subregion mit dem grössten Flächenanteil gewählt.
     */
    ASSOCIATION Vereinbarung_UZL_Subregion =
      Vereinbarung -- {0..*} Vereinbarung;
      UZL_Subregion -- {1} UZL_Subregion;
    END Vereinbarung_UZL_Subregion;

  END MJPNL;

END SO_ARP_MJPNL_20201026.
