      !!@ ili2db.dispName = "Jahr der Auszahlung"
      Auszahlungsjahr : MANDATORY 1900 .. 2100;
      !!@ ili2db.dispName = "Status der Abrechnung"
      Status_Abrechnung : MANDATORY Abrechnung_Status;
