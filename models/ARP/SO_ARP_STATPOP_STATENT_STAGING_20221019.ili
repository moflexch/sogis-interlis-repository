  TOPIC STATPOP_STATENT =
    OID AS INTERLIS.UUIDOID;

    /** STATENT-Daten (Unternehmen)
     */
    CLASS STATENT =
      /** Beschäftigte total.
       */
      Emptot : MANDATORY 0 .. 1000000;
      /** Beschäftigte inkl Beschäftigungsgrad
       */
