      /** Y Koordinate (EPSG: 2056, LV95)
       */
      Meter_Y : MANDATORY 0 .. 2000000;
      /** Ein Code für irgendwas
       */
      NOGA08_CD : MANDATORY 0 .. 1000000000;
    END STATENT;

    /** Bevölkerungsdaten
