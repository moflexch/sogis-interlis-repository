      /** Federal Building Pseudo ID
       */
      FederalBuildingIdPseudo : MANDATORY TEXT*2000;
      /** E-Koordinate, X Koordinate (EPSG: 2056, LV95)
       */
      GeocoordE : MANDATORY 0 .. 3000000;
      /** N-Koordinate, Y Koordinate (EPSG: 2056, LV95)
       */
