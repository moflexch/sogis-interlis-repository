      autre,
      peinture_revetement,
      revetement_en_brique,
      revetement_en_gres,
      inconnue,
      revetement_en_mortier_de_ciment
    );
    !!@ comment = "Longueur effective (non projetée) incluant les parties incurvées"
    L_EFFECTIVE: 0.00 .. 30000.00 [m];
