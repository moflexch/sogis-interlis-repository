CLASS Ueberlauf_Foerderaggregat EXTENDS SIA405_Base_LV95.SIA405_BaseClass =  
!! Topologische Verbindung von zwei Knoten zur Abbildung von Wehren und Pumpen
  ATTRIBUTE
    Art: (          !! Art des Überlaufs bzw. Förderaggregats
      Foerderaggregat,
      Leapingwehr,
      Streichwehr
    );
    Bezeichnung: MANDATORY TEXT*20;
UNIQUE 
