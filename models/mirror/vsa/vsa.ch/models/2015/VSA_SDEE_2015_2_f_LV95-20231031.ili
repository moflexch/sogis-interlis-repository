CLASS RIVE EXTENDS SIA405_Base_f_LV95.SIA405_BaseClass =  
!! Zone bordant un cours d'eau ou un lac
  ATTRIBUTE
    REMARQUE: TEXT*80;  !! Remarques générales
    DESIGNATION: MANDATORY TEXT*20;
    LARGEUR: 0.00 .. 30000.00 [m];  !! Distance horizontale de la zone comprise entre le pied de la berge et la zone d'utilisation intensive du sol
    COTE: (          !! Berges sur le côté gauche ou droite du cours d'eau par rapport au sens d'écoulement
      gauche,
      droite,
      inconnu
    );
    RIVES: (          !! Nature de la zone en dessus du pied de la berge du cours d'eau
      atypique_d_un_cours_d_eau,
      typique_d_un_cours_d_eau,
      artificielle,
      inconnu
    );
    UTILISATION_SOL_ENVIRONS: (          !! Utilisation du sol des environs
      constructions,
      espaces_verts,
      inconnu,
      foret
    );
    VEGETATION: (        
      absente,
      typique_du_lieu,
      non_typique_du_lieu,
      inconnu
    );
    GENRE_AMENAGEMENT: (          !! Genre d'aménagement du pied de la berge du cours d'eau
      autres_impermeables,  !! (imperméable)
      brique_perforee_en_beton_impermeable,  !! (imperméable)
      bois_permeable,  !! (perméable)
      aucun_amenagement,  !! (perméable)
      materiau_vegetal_permeable,  !! (perméable)
      mur_impermeable,  !! (imperméable)
      pierre_naturelle_impermeable,  !! (imperméable)
      pierre_naturelle_lache,  !! (perméable)
      inconnu  !! (perméable)
    );
    DEGRE_AMENAGEMENT: (          !! Degré d'aménagement du pied du talus du cours d'eau
      nul,  !! nul (0%)
      moyen,  !! moyen (10 - 30 %)
      fort,  !! fort (30 - 60 %)
      preponderant,  !! prépondérant (> 60 %)
      inconnu,
      localise,  !! localisé (<10%)
      total  !! total (100 %)
    );
END RIVE;

ASSOCIATION RIVE_TRONCON_COURS_EAUAssoc =    !! Composition
  TRONCON_COURS_EAURef  -<#> {1} TRONCON_COURS_EAU; !! Rolle1 - Klasse1 / Rôle1 - Classe1
  RIVE_TRONCON_COURS_EAUAssocRef -- {0..2} RIVE; !! , Rolle2 - Klasse2 / Rôle2 - Classe2
