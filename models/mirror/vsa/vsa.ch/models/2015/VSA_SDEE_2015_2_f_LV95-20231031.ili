CLASS MUTATION EXTENDS SIA405_Base_f_LV95.SIA405_BaseClass =  
!! Transfert des mutations des valeurs d'attributs
  ATTRIBUTE
    GENRE: (        
      cree,
      changee,
      effacee
    );
    ATTRIBUT: TEXT*50;  !! Nom de l'attribut de l'objet à sélectionner
    DATE_DE_RELEVE: INTERLIS_1_DATE;  !! Date/temps de la relève, sinon date/temps de création dans le système
    RELEVE_PAR: SIA405_Base_f_LV95.Organisation_Designation;  !! Nom de la personne, qui a relevé les données
    REMARQUE: TEXT*80;  !! Remarques générales
    CLASSE: TEXT*50;  !! Nom de classe de l'objet à sélectionner
    DERNIERE_VALEUR: TEXT*100;  !! Dernière valeur modifiée du texte. Seulement avec GENRE = changee ou effacee
    DATE_MUTATION: INTERLIS_1_DATE;  !! changée: Date/Temps du changement. effacée: Date/Temps de la suppression
    OBJET: TEXT*41;  !! OBJ_ID de l'objet
    USAGER_SYSTEME: TEXT*41;  !! Usager du système informatique
END MUTATION;

