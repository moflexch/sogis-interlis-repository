      fort,  !! fort (30 - 60 %)
      preponderant,  !! prépondérant  (> 60 %)
      inconnu,
      localise,  !! localisé (<10%)
      total  !! total (100 %)
    );
END FOND_COURS_EAU;

ASSOCIATION FOND_COURS_EAU_TRONCON_COURS_EAUAssoc =    !! Composition
