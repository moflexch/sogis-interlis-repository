    GENRE: (          !! Type de fond
      dur,
      inconnu,
      tendre
    );
    REMARQUE: TEXT*80;  !! Remarques générales
    DESIGNATION: MANDATORY TEXT*20;
    LARGEUR: 0.00 .. 30000.00 [m];  !! Zone de charriage par hautes eaux (absence de plantes aquatiques supérieures);
    GENRE_AMENAGEMENT_LIT: (          !! Genre d'aménagement du fond
      autres_impermeables,  !! (imperméable)
      briques_perforees_en_beton,  !! (imperméable)
      bois,  !! (imperméable)
      aucun_amenagement,  !! (perméable)
      pierres_naturelles,  !! (perméable)
      inconnu  !! (perméable)
    );
    DEGRE_AMENAGEMENT_LIT: (          !! Pourcentage de la surface avec aménagement du fond du lit. Classification
      nul,  !! nul (0%)
