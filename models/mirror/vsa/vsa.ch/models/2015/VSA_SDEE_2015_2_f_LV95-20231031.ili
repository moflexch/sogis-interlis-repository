CLASS TRONCON_COURS_EAU EXTENDS SIA405_Base_f_LV95.SIA405_BaseClass =  
!! Partie d'un cours d'eau homogène du point de vue morphologique
  ATTRIBUTE
    REGIME_ECOULEMENT: (          !! Degré d'intervention anthropogène sur le régime hydraulique
      modifie,
      artificiel,
      peu_naturel,
      presque_naturel,
      inconnu
    );
    ALGUES: (          !! Couverture végétale par des algues
      absent_faible,
      moyen_fort,
      tres_fort_proliferation,
      inconnu
    );
    GENRE: (        
      mis_sous_terre,
      ouvert,
      inconnu
    );
    REMARQUE: TEXT*80;  !! Remarques générales
    DESIGNATION: MANDATORY TEXT*20;
    A: Base_f_LV95.CoordNat;  !! Situation de la fin du tronçon
    VARIABILITE_LARGEUR: (          !! Variabilité de la largeur du lit mouillé par basses et moyennes eaux
      prononcee,
      limitee,
      nulle,
      inconnu
    );
    PENTE: (          !! Pente moyenne du fond du tronçon cours d'eau
      plat,  !! < 1%
      moyen,  !! 1 - 3%
      raide,  !! > 3%
      inconnu
    );
    RANG: Strahler;  !! Classification selon Strahler
    ETAGE_ALTITUDE: (          !! Type d'étage d'altitude des cours d'eau
      alpin,
      des_collines,
      montagnard,
      subalpin,
      inconnu
    );
    PROFIL_EN_LONG: (          !! Caractérisation du profil en long
      avec_des_cascades,
      avec_rapides_marmites,
      continu,
      inconnu
    );
    LIGNE_TRACE: (          !! Tracé d'un cours d'eau
      rectiligne,
      legerement_incurve,  !! légèrement courbé
      en_meandres,
      fortement_incurve,  !! fortement courbé
      inconnu
    );
    MACROPHYTES: (          !! Couverture végétale par des macrophytes (végétation aquatique (macroscopique))
      absent_faible,
      moyen_fort,
      tres_fort_proliferation,
      inconnu
    );
    UTILISATION: (          !! Utilisation primaire du tronçon de cours d'eau
      detente,
      peche,
      installation_de_retenue,
      inconnu
    );
    CLASSIFICATION_ECOMORPH: (          !! Attribut issu de la classification écomorphologique du niveau R
      mis_sous_terre,
      artificiel_peu_naturel,
      naturel_presque_naturel,
      pas_classifie,
      fortement_modifie,
      peu_modifie
    );
    LARGEUR_LIT: 0.00 .. 30000.00 [m];  !! Largeur moyenne du lit
    VAR_PROFONDEUR: (          !! Variabilité de la profondeur d'eau
      prononcee,
      aucune,
      moyenne,
      inconnu
    );
    BOIS_MORT: (          !! Amas de bois mort dans le cours d'eau
      amas,
      absent_localise,
      inconnu,
      dissemine
    );
    DE: Base_f_LV95.CoordNat;  !! Situation du début du tronçon
    DURETE_EAU: (          !! Dureté chimique de l'eau
      calcaire,
      silicieuse,
      inconnu
    );
END TRONCON_COURS_EAU;

ASSOCIATION TRONCON_COURS_EAU_COURS_EAUAssoc =    !! Composition
