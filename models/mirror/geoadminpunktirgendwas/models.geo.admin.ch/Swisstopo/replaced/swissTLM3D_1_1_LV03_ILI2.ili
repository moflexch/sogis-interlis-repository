      TLM_Name_Name_UUID: UUIDOID;
    END TLM_SIEDLUNGSNAME;

    CLASS TLM_FLURNAME EXTENDS TLM_OBJEKT =
      Objektart: MANDATORY (Flurname, Lokalname_swisstopo);
      Shape: MANDATORY HKoord;
      Name: TEXT*1000;
      TLM_Name_Name_UUID: UUIDOID;
    END TLM_FLURNAME;

    CLASS TLM_GEBIETSNAME EXTENDS TLM_OBJEKT =
