  DOMAIN

    LKoord = COORD
      2480000.000 .. 2850000.000 [m] {CHLV95[1]},
      1070000.000 .. 1310000.000 [m] {CHLV95[2]},
      ROTATION 2 -> 1;

    HKoord = COORD
      2480000.000 .. 2850000.000 [m] {CHLV95[1]},
      1070000.000 .. 1310000.000 [m] {CHLV95[2]},
         -200.000 ..   5000.000 [m] {SwissOrthometricAlt[1]},
      ROTATION 2 -> 1;

    NameTyp = TEXT*30;

    NummerTyp = TEXT*12;
    NBNummer = TEXT*12;

    Zuverlaessigkeit = BOOLEAN;
      !! True = genuegend
      !! False = ungenuegend

    Status = (
      projektiert,
      gueltig:FINAL);

    Hoehe = -200.000 .. 5000.000 [m];

    Genauigkeit = 0.0 .. 700.0 [Units.cm];

    Rotation = 0.0 .. 399.9 [Units.Gon];

    Qualitaetsstandard = (
      AV93,
      PV74,
      PN,
      PEP, !! Provisorischen Ersatzprodukte
      weitere:FINAL);

    Versicherungsart = (
      Stein,
      Kunststoffzeichen,
      Bolzen,
      Rohr,
      Pfahl,
      Kreuz,
      unversichert,
      weitere:FINAL);

    Status_GA = ( !! Fuer Gebaeudeadressen, siehe SN 612040
      projektiert,    !! neues Objekt, in Bearbeitung
                      !! Zustand zufaellig
      real,           !! Objekt existiert real
      vergangen:FINAL);     !! Objekt existiert nicht mehr

    Sprachtyp = ( !! Fuer Gebaeudeadressen, siehe SN 612040
      de,  !! deutsch
      fr,  !! francais
      it,  !! italiano
      rm,  !! rumantsch
      en:FINAL);  !! english

    Schriftgroesse = (
      klein,
      mittel,
      gross:FINAL);

    Schriftstil = (
      normal,
      gesperrt,
      weitere:FINAL);

    UUIDOID = OID TEXT*36;  !! gemäss ISO 11578

!! Die OIDs kennzeichnen ein Objekt innerhalb der Transfergemeinschaft
