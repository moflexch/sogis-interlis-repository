    END Landesgrenzabschnitt;

  END Landesgrenzen;



  TOPIC Planeinteilungen =

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    CLASS Plan =
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Nummer: MANDATORY NummerTyp;
      TechDossier: MANDATORY NummerTyp;
      GueltigerEintrag: XMLDate;
    UNIQUE NBIdent, Nummer;
    END Plan;

    CLASS Plangeometrie =
        Geometrie: MANDATORY AREA WITH (STRAIGHTS, ARCS) VERTEX LKoord
          WITHOUT OVERLAPS > 0.050;
    END Plangeometrie;

    ASSOCIATION PlangeometrieBez =
      Plangeometrie_von -<#> {1} Plan;
      Geometrie -- {1..*} Plangeometrie;
    END PlangeometrieBez;

    CLASS PlanPos =
      Pos: MANDATORY LKoord; !! Beschriftet Nummer
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = Half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
    END PlanPos;

    ASSOCIATION PlanPosBez =
      PlanPos_von -<#> {1} Plan;
      Pos -- {0..*} PlanPos;
    END PlanPosBez;

  END Planeinteilungen;



  TOPIC TSEinteilung =

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    CLASS Toleranzstufe =
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY NummerTyp;  !! Nummer des technischen Dossiers
      Geometrie: MANDATORY AREA WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.200;
      GueltigerEintrag: XMLDate;
      Art (FINAL): MANDATORY (
        TS1,
        TS2,
        TS3,
        TS4,
        TS5);
    UNIQUE NBIdent, Identifikator;
    END Toleranzstufe;

    CLASS ToleranzstufePos =
      Pos: MANDATORY LKoord; !! Beschriftet art
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
    END ToleranzstufePos;

    ASSOCIATION ToleranzstufePosBez =
      ToleranzstufePos_von -<#> {1} Toleranzstufe;
      Pos -- {0..*} ToleranzstufePos;
      MANDATORY CONSTRAINT CoordIn(Pos->Pos, ToleranzstufePos_von->Geometrie);
    END ToleranzstufePosBez;

  END TSEinteilung;



  TOPIC Rutschgebiete =

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    CLASS Rutschung =
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY NummerTyp;  !! Nummer des technischen Dossiers
      Name: NameTyp;  !! eigener Name
      Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.200;
      GueltigerEintrag: XMLDate;
    UNIQUE NBIdent, Identifikator;
    END Rutschung;

    CLASS RutschungPos =
      Pos: MANDATORY LKoord; !! Beschriftet Pos
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = Half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
    END RutschungPos;

    ASSOCIATION RutschungPosBez =
        RutschungPos_von -<#> {1} Rutschung;
        Pos -- {0..*} RutschungPos;
      MANDATORY CONSTRAINT CoordIn(Pos->Pos, RutschungPos_von->Geometrie);
    END RutschungPosBez;

  END Rutschgebiete;



 TOPIC PLZOrtschaft = !! siehe SN 612040;
                     !! Verantwortung der Kantone fuer Ortschaften
                     !! Verantwortung der Post fuer PLZ
    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    CLASS OSNachfuehrung =
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY NummerTyp;
      Beschreibung: MANDATORY NameTyp;
      Perimeter: SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
         WITHOUT OVERLAPS > 0.500;
      Gueltigkeit: MANDATORY Status;
      GueltigerEintrag:  MANDATORY XMLDate;
    UNIQUE NBIdent, Identifikator;
    END OSNachfuehrung;

    CLASS OrtschaftsVerbund =
      Leer: TEXT*1;
    END OrtschaftsVerbund;

    CLASS OrtschaftsVerbundText=
      Text: MANDATORY TEXT*200;
      Sprache: MANDATORY Sprachtyp;
    END OrtschaftsVerbundText;

    ASSOCIATION OrtschaftsVerbund_OrtschaftsVerbundTextBez =
      OrtschaftsVerbund_von -<#> {1} OrtschaftsVerbund;
      Von_OrtschaftsVerbundText -- {1..*} OrtschaftsVerbundText;
      UNIQUE OrtschaftsVerbund_von, Von_OrtschaftsVerbundText->Sprache;
    END OrtschaftsVerbund_OrtschaftsVerbundTextBez;

    !! Die realen Ortschaften bilden AREA
    CLASS Ortschaft=
      Status: MANDATORY Status_GA;
      InAenderung: MANDATORY BOOLEAN;
      Flaeche: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.500;
    END Ortschaft;

    ASSOCIATION OrtschaftNachfBez =
      Entstehung -- {1} OSNachfuehrung;
      Von_Ortschaft -- {0..*} Ortschaft;
    END OrtschaftNachfBez;

    ASSOCIATION VerbundZugehoerigkeitBez =
      Verzeichnis -- {0..1} OrtschaftsVerbund;
      BeteiligteOrtschaft -- {1..*} Ortschaft;
    END VerbundZugehoerigkeitBez;

    !! Beispiel fuer Text, KurzText und IndexText:
    !! Text (Vollstaendiger Name): La Chaux-de-Fonds
    !! KurzText (Schreibweise in der Postadresse): La Chx-de-Fds
    !! IndexText (Fuer Index): Chaux-de-Fonds
   CLASS OrtschaftsName =
      Text: MANDATORY TEXT*40;
      KurzText: TEXT*18;
      IndexText: TEXT*16;
      Sprache: MANDATORY Sprachtyp;
    END OrtschaftsName;

    ASSOCIATION Ortschafts_OrtschaftsNameBez =
      Von_Ortschaft -<#> {1} Ortschaft;
      Namen -- {1..*} OrtschaftsName;
      UNIQUE Von_Ortschaft, Namen->Sprache;
    END Ortschafts_OrtschaftsNameBez;

    CLASS OrtschaftsName_Pos =
      Pos: MANDATORY LKoord; !! Beschriftet Text
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = half
      Groesse: Schriftgroesse; !! undefiniert = mittel
    END OrtschaftsName_Pos;

    ASSOCIATION OrtschaftsName_PosBez =
        Ortschafts_von -<#> {1} Ortschaft;
        NamenPos -- {0..*} OrtschaftsName_Pos;
    END OrtschaftsName_PosBez;

    CLASS PLZ6Nachfuehrung =
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY NummerTyp;
      Beschreibung: MANDATORY NameTyp;
      Perimeter: SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
         WITHOUT OVERLAPS > 0.500;
      Gueltigkeit: MANDATORY Status;
      GueltigerEintrag:  MANDATORY XMLDate;
    UNIQUE NBIdent, Identifikator;
    END PLZ6Nachfuehrung ;

    !! Eine reale PLZ6 auf eine nicht reale Ortschaft ist zu vermeiden
    CLASS PLZ6=
      !! Sind einer Ortschaft mehrere sechsstellige Postleitzahlen zugeordnet,
      !! muss fuer jede derselben eine Flaeche definiert sein, und alle diese
      !! Flaechen muessen innerhalb der Flaeche der Ortschaft liegen.
      !! Die realen PLZ6 sind vom Typ AREA.
      Flaeche: SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.500;
      Status: MANDATORY Status_GA;
      InAenderung: MANDATORY BOOLEAN;
      PLZ: MANDATORY 1000..9999;
      Zusatzziffern: MANDATORY 00..99;
    UNIQUE PLZ, Zusatzziffern;
    END PLZ6;

    ASSOCIATION PostleitzahlBez =
      OrtschaftZuPLZ -- {1} Ortschaft;
      PLZ -- {*} PLZ6;
    END PostleitzahlBez;

    ASSOCIATION PLZ6NachfBez =
      Entstehung -- {1} PLZ6Nachfuehrung;
      Von_PLZ6 -- {0..*} PLZ6;
    END PLZ6NachfBez;

END PLZOrtschaft;



TOPIC Gebaeudeadressen =  !! siehe SN 612040;
                          !! Verantwortung der Gemeinden

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    !! Geometrische Beziehung zwischen Gebaeudeeingang und PLZ
    !! Geometrische Beziehung zwischen Lokalisation und LokalisationnenVerz
    !! Geometrische Beziehung zwischen Gebaeudeeingang und BennantesGebiet

    CLASS GEBADRNachfuehrung =
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY NummerTyp;
      Beschreibung: MANDATORY NameTyp;
      Perimeter: SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
         WITHOUT OVERLAPS > 0.500;
      Gueltigkeit: MANDATORY Status;
      GueltigerEintrag:  MANDATORY XMLDate;
    UNIQUE NBIdent, Identifikator;
    END GEBADRNachfuehrung;

    CLASS Lokalisation =
      Nummerierungsprinzip: MANDATORY (
        keineNummern,
        beliebig,
        aufsteigend,
        ungeradelinks,
        geradelinks:FINAL);
      LokalisationNummer: TEXT*12;
      AttributeProvisorisch: MANDATORY BOOLEAN;
      IstOffizielleBezeichnung: MANDATORY BOOLEAN;
      Status: MANDATORY Status_GA;
      InAenderung: MANDATORY BOOLEAN;
      Art: MANDATORY (
          BenanntesGebiet,
          Strasse,
          Platz:FINAL);
    END Lokalisation;

    ASSOCIATION LokalisationNachfBez = !! DM01: zusaetzlich zur SN
      Entstehung -- {1} GEBADRNachfuehrung;
      Von_Lokalisation -- {0..*} Lokalisation;
    END LokalisationNachfBez;

    !! Beispiel fuer Text, KurzText und IndexText:
    !! Text (Vollstaendiger Name): Conrad-Ferdinand-Meyer-Strasse
    !! KurzText (Schreibweise in der Postadresse): CF Meyer Str
    !! IndexText (Fuer Index): Meyer CF Str
    CLASS LokalisationsName =
      Text: MANDATORY TEXT*60;
      KurzText: TEXT*24;
      IndexText: TEXT*16;
      Sprache: MANDATORY Sprachtyp;
    END LokalisationsName;

    ASSOCIATION Lokalisation_LokalisationsNameBez =
      Benannte -<#> {1} Lokalisation;
      Von_Lokalisation -- {1..*} LokalisationsName;
      UNIQUE Benannte, Von_Lokalisation->Sprache;
    END Lokalisation_LokalisationsNameBez;

    CLASS LokalisationsName_Pos =
      AnfIndex: 1 .. 60; !! undefiniert = 1
      EndIndex: 1 .. 60; !! undefiniert = letztes Zeichen
      Pos: MANDATORY LKoord; !! Beschriftet Text
      Ori: Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = half
      Groesse: Schriftgroesse; !! undefiniert = mittel
      Hilfslinie: POLYLINE WITH (STRAIGHTS) VERTEX LKoord;
    END LokalisationsName_Pos;

    ASSOCIATION LokalisationsName_PosBez =
      Name_Pos_von -<#> {1} LokalisationsName;
      Von_Pos -- {0..*} LokalisationsName_Pos;
    END LokalisationsName_PosBez;

    CLASS BenanntesGebiet =
      Flaeche: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.500;
    END BenanntesGebiet;

    ASSOCIATION BenanntesGebiet_LokalisationBez =
      Von_Lokalisation -<#> {1} Lokalisation;
      Von_BenanntesGebiet -- {0..*} BenanntesGebiet;
      MANDATORY CONSTRAINT
      Von_Lokalisation -> Art==#BenanntesGebiet;
    END BenanntesGebiet_LokalisationBez;

    !! Die Geometrie entspricht der Strassenachse,
    !! siehe die Erklaerungen, Kapitel 3.18.2
    !! Hauptstrassenabschnitt. Die privaten Zugangsabschnitte
    !! werden nicht im Bundesmodell erfasst
    CLASS Strassenstueck =
      Geometrie: MANDATORY POLYLINE WITH (STRAIGHTS, ARCS) VERTEX LKoord;
      Anfangspunkt: LKoord; !! DM01: zusaetzlich zur SN
      Ordnung: MANDATORY 1 .. 999;  !! Reihenfolge der Strassenstuecke
      istAchse: MANDATORY BOOLEAN;
    END Strassenstueck;

    ASSOCIATION Strassenstueck_LokalisationBez =
      Von_Lokalisation -<#> {1} Lokalisation;
      Von_Strassenstueck -- {0..*} Strassenstueck;
      MANDATORY CONSTRAINT
      Von_Lokalisation -> Art==#Strasse OR Von_Lokalisation -> Art==#Platz;
    END Strassenstueck_LokalisationBez;

    CLASS GebaeudeEingang =
      Status: MANDATORY Status_GA;
      InAenderung: MANDATORY BOOLEAN;
      AttributeProvisorisch: MANDATORY BOOLEAN;
      IstOffizielleBezeichnung: MANDATORY BOOLEAN;
      Lage: MANDATORY LKoord;
      !! HoehenLage ist nuetzlich wenn mehrere Eingaenge auf mehreren Niveaux
      !! Ungefaehre Hoehe ueber gewachsenem Terrain
      Hoehenlage: -99 .. 99 [m]; !! Ungefähre Höhe über gewachsenem Terrain
      !! Die Hausnummer besteht aus einer Nummer,
      !! welche mit einem Zusatz (Buchstaben a,b,c)
      !! ergaenzt werden kann. Zwischen Nummer und Zusatz
      !! keine Leerschlaege oder Trennzeichen.
      !! Wenn die Hausnummer definiert ist, dann muss fuer Lokalisation und
      !! Gebaeudeeingang gelten:
      !! - Lokalisation und Hausnummer muessen fuer Status = real
      !!   zusammen eindeutig sein;
      !! - Nummerierungsprinzip darf nicht den Wert keines haben.
      Hausnummer: TEXT*12;
      !! Im_Gebaeude ist nuetzlich, um zu definieren, ob die Nummer mit einem
      !! Objekt der BB oder mit einem Objekt der EO verknuepft ist.
      Im_Gebaeude (FINAL): MANDATORY (BB, EO); !! DM01: zusaetzlich zur SN
      !! Eidg. Gebaeude-Identifikator wenn verfuegbar,
      !! siehe Erklaerung Kapitel 3.18.2
      GWR_EGID: 1 .. 999999999;
      !! Eidg. Eingang-Identifikator wenn verfuegbar,
      !! siehe Erklaerung Kapitel 3.18.2
      GWR_EDID: 0 .. 99 ; !! DM01: zusaetzlich zur SN
    END GebaeudeEingang;

    ASSOCIATION GebaeudeEingangNachfBez = !! DM01: zusaetzlich zur SN
      Entstehung -- {1} GEBADRNachfuehrung;
      Eingang -- {0..*} GebaeudeEingang;
    END GebaeudeEingangNachfBez;

    ASSOCIATION EingangLokalisationBez =
      GebaeudeEingang_von -<> {0..1} Lokalisation;
      GebaeudeEingaenge -- {0..*} GebaeudeEingang;
    MANDATORY CONSTRAINT
      ((GebaeudeEingang_von->Nummerierungsprinzip != #keineNummern) ==
      (DEFINED(GebaeudeEingaenge->Hausnummer)));
    END EingangLokalisationBez;

    CLASS Hausnummer_Pos =
      Pos: MANDATORY LKoord; !! Beschriftet Hausnummer
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = half
      Groesse: Schriftgroesse; !! undefiniert = mittel
    END Hausnummer_Pos;

    ASSOCIATION GebaeudeEingang_PosBez =
        Von_GebaeudeEingang -<#> {1} GebaeudeEingang;
        PosNummer -- {0..*} Hausnummer_Pos;
    END GebaeudeEingang_PosBez;

    CLASS GebaeudeName =
      Text: MANDATORY TEXT*40;
      KurzText : TEXT*24;
      IndexText: TEXT*16;
      Sprache: MANDATORY Sprachtyp;
    END GebaeudeName;

    ASSOCIATION GebaeudeName_GebaeudeEingangBez =
        Von_GebaeudeEingang -<#> {1} GebaeudeEingang;
        Hat_GebaeudeName -- {0..*} GebaeudeName;
        UNIQUE Von_GebaeudeEingang, Hat_GebaeudeName->Sprache;
    END GebaeudeName_GebaeudeEingangBez;

    CLASS GebaeudeName_Pos =
      Pos: MANDATORY LKoord; !! Beschriftet Text
      Ori: Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = half
      Groesse: Schriftgroesse; !! undefiniert = mittel
      Hilfslinie: POLYLINE WITH (STRAIGHTS) VERTEX LKoord;
    END GebaeudeName_Pos;

    ASSOCIATION GebaeudeName_PosBez =
      Von_GebaeudeName -<#> {1} GebaeudeName;
      Pos -- {0..*} GebaeudeName_Pos;
    END GebaeudeName_PosBez;

    CLASS GebaeudeBeschreibung =
      Text: MANDATORY TEXT*100;
      Sprache: MANDATORY Sprachtyp;
    END GebaeudeBeschreibung;

    ASSOCIATION GebaeudeBeschreibung_GebaeudeEingangBez =
        Von_GebaeudeEingang -<#> {1} GebaeudeEingang;
        Hat_GebaeudeBeschreibung -- {0..*} GebaeudeBeschreibung;
        UNIQUE Von_GebaeudeEingang, Hat_GebaeudeBeschreibung->Sprache;
    END GebaeudeBeschreibung_GebaeudeEingangBez;

  END Gebaeudeadressen;



  TOPIC Planrahmen =
  !! Die in der Technischen Verordnung ueber die AV erwaehnten Objekte muessen
  !! verwaltet werden.

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    DOMAIN


      Massstabstyp = 1 .. 1000000;

      Beschriftungsart  = (
        Nachbarn,              !! Gemeinde, Bezirk, Kanton oder Land
        Nachbarplan,           !! Nachbarplaene in der Situation
        UebersichtNachbarn,    !! Nachbarplaene, Gemeinde, Bezirk,
                               !! Kanton oder Land im Uebersichtsfenster
        Strassenrichtung,
        LK_Nr,
        BFSNr,
        weitere:FINAL);              !! nur fuer definierte Erweiterungen

      Linientyp = (
        standard,
        weitere:FINAL);

      Symbolart = (
        Nordpfeil,
        weitere:FINAL);

      Kreuzart = (
        Koord_Kreuz,
        Netzkreuz,
        Netzmarkierung,
        weitere:FINAL);


    CLASS PlanLayout =
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY TEXT*32;
      Layouttyp: MANDATORY TEXT*20;  !! Definition des verwendeten Planspiegels
      Plannummer: MANDATORY NummerTyp;
      Gemeindename: MANDATORY NameTyp;
      Geometername: NameTyp;
      Erstellungsdatum: MANDATORY XMLDate;
      NachfuehrungsGeometername: NameTyp;
      Nachfuehrungsdatum: XMLDate;
      Massstabszahl: MANDATORY Massstabstyp;
      Plannullpunkt: MANDATORY LKoord;
      E_Azimut: MANDATORY Rotation;  !! Azimut 100 ist E
      UebersichtMassstabszahl: Massstabstyp;
      UebersichtPlannullpunkt: LKoord;
      Mit_Koordinatennetz: MANDATORY BOOLEAN;
        !! True    mitgeliefert
        !! False  zu generieren
    UNIQUE NBIdent, Identifikator;
    END PlanLayout;

    CLASS Planbeschriftung =
      Beschriftung: MANDATORY NameTyp;
      Art: MANDATORY Beschriftungsart;
    END Planbeschriftung;

    ASSOCIATION PlanbeschriftungPlanLayoutBez =
      Planbeschriftung_von -<#> {1} PlanLayout;
      Beschriftung -- {0..*} Planbeschriftung;
    END PlanbeschriftungPlanLayoutBez;

    CLASS PlanbeschriftungPos =
      Pos: MANDATORY LKoord; !! Beschriftet Beschriftung
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
    END PlanbeschriftungPos;

    ASSOCIATION PlanbeschriftungPosBez =
      PlanbeschriftungPos_von -<#> {1} Planbeschriftung;
      Pos -- {1..*} PlanbeschriftungPos;
    END PlanbeschriftungPosBez;

    CLASS Koordinatenanschrift =
      Beschriftung: MANDATORY NameTyp;
    END Koordinatenanschrift;

    ASSOCIATION KoordinatenanschriftBez =
      Koordinatenanschrift_von -<#> {1} PlanLayout;
      Anschrift -- {0..*} Koordinatenanschrift;
    END KoordinatenanschriftBez;

    CLASS KoordinatenanschriftPos =
      Pos: MANDATORY LKoord; !! Beschriftet Beschriftung
      Ori: Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
    END KoordinatenanschriftPos;

    ASSOCIATION KoordinatenanschriftPosBez =
      KoordinatenanschriftPos_von -<#> {1} Koordinatenanschrift;
      Position -- {1..*} KoordinatenanschriftPos;
      UNIQUE KoordinatenanschriftPos_von, Position->Pos;
    END KoordinatenanschriftPosBez;

    CLASS Linienobjekt =
      Geometrie: MANDATORY POLYLINE WITH (STRAIGHTS, ARCS) VERTEX LKoord;
      Art: MANDATORY Linientyp;
    END Linienobjekt;

    ASSOCIATION LinienobjektBez =
      Linienobjekt_von -<#> {1} PlanLayout;
      Linienobjekte -- {0..*} Linienobjekt;
    END LinienobjektBez;

    CLASS KoordinatenLinie =
      Geometrie: MANDATORY POLYLINE WITH (STRAIGHTS) VERTEX LKoord;
    END KoordinatenLinie;

    ASSOCIATION KoordinatenLinieBez =
      KoordinatenLinie_von -<#> {1} PlanLayout;
      KoordinatenLinien -- {0..*} KoordinatenLinie;
    END KoordinatenLinieBez;

    CLASS Darstellungsflaeche =
      Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.050;
      Auswahlart: MANDATORY (
        vollstaendig,
        teilweise:FINAL);
    END Darstellungsflaeche;

    ASSOCIATION DarstellungsflaecheBez =
      Darstellungsflaeche_von -<#> {1} PlanLayout;
      Darstellungsflaechen -- {0..*} Darstellungsflaeche;
    END DarstellungsflaecheBez;

    CLASS PlanLayoutSymbol =
      Pos: MANDATORY LKoord;
      Ori: Rotation;    !! undefiniert = 0.0
      Art: MANDATORY Symbolart;
    END PlanLayoutSymbol;

    ASSOCIATION PlanLayoutSymbolBez =
      PlanLayoutSymbol_von -<#> {1} PlanLayout;
      Symbol -- {0..*} PlanLayoutSymbol;
    END PlanLayoutSymbolBez;

    CLASS Netzkreuz =
      Pos: MANDATORY LKoord;
      Ori: Rotation;    !! undefiniert = 0.0
      Art: MANDATORY Kreuzart;
    END Netzkreuz;

    ASSOCIATION NetzkreuzBez =
      Netzkreuz_von -<#> {1} PlanLayout;
      Netzkreuze -- {0..*} Netzkreuz;
    UNIQUE Netzkreuz_von, Netzkreuze->Pos;
    END NetzkreuzBez;

  END Planrahmen;



END DM01AVCH24LV95D_ili2.

