  TOPIC FixpunkteKategorie1 =

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    CLASS LFP1Nachfuehrung=
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY NummerTyp;
      Beschreibung: MANDATORY NameTyp;
      Perimeter: SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
         WITHOUT OVERLAPS > 0.200;
      !! Fuer zukuenftige Nachfuehrungen ist das Attribut GueltigerEintrag zu
      !! erfassen. Datum1 betrifft die alten Nachfuehrungen und wird nicht mehr
      !! angewendet. In der naechsten Revision des Datenmodells wird das Attribut
      !! Datum1 geloescht und das Attribut GueltigerEintrag wird obligatorisch.
      GueltigerEintrag: XMLDate;
      Datum1: XMLDate;
    UNIQUE NBIdent, Identifikator;
    END LFP1Nachfuehrung;

    !! Ist der LFP1 auch Hoheitsgrenzpunkt, so muss Punktzeichen
    !! angegeben werden, da dieser Punkt in
    !! Gemeindegrenzen.Hoheitsgrenzpunkt kopiert wird.
    CLASS LFP1 =
      NBIdent: MANDATORY NBNummer; !! Beziehung 1-m zu Nummerierungsbereich
      Nummer: MANDATORY NummerTyp;
      Geometrie: MANDATORY LKoord;
      HoeheGeom: Hoehe;
      LageGen: MANDATORY Genauigkeit;
      LageZuv: MANDATORY Zuverlaessigkeit;
      HoeheGen : Genauigkeit;
      HoeheZuv : Zuverlaessigkeit;
      Begehbarkeit : MANDATORY (
          begehbar,
          nicht_begehbar:FINAL);
      Punktzeichen: Versicherungsart; !! Nur unversichert nicht zugelassen
      MANDATORY CONSTRAINT ((NOT (DEFINED (Punktzeichen)) OR Punktzeichen <> #unversichert));
    UNIQUE NBIdent, Nummer;
    END LFP1;

    ASSOCIATION LFP1Bez =
      Entstehung -- {1} LFP1Nachfuehrung;
      Von_Punkte -- {0..*} LFP1;
    END LFP1Bez;

    CLASS LFP1Pos =
      Pos: MANDATORY LKoord; !! Beschriftet Nummer :
      Ori: Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Left
      VAli: VALIGNMENT; !!  VAli undefiniert = Bottom
    END LFP1Pos;

    ASSOCIATION LFP1PosBez =
      LFP1Pos_von -<#> {1} LFP1;
      Pos -- {1} LFP1Pos;
    END LFP1PosBez;

    CLASS LFP1Symbol =
      Ori: Rotation; !! undefiniert = 0.0
    END LFP1Symbol;

    ASSOCIATION LFP1SymbolBez =
      LFP1Symbol_von -<#> {1} LFP1;
      Symbol-- {0..1} LFP1Symbol;
    END LFP1SymbolBez;

    CLASS HFP1Nachfuehrung =
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY NummerTyp;
      Beschreibung: MANDATORY NameTyp;
      Perimeter: SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
         WITHOUT OVERLAPS > 0.200;
      !! Fuer zukuenftige Nachfuehrungen ist das Attribut GueltigerEintrag zu
      !! erfassen. Datum1 betrifft die alten Nachfuehrungen und wird nicht mehr
      !! angewendet. In der naechsten Revision des Datenmodells wird das Attribut
      !! Datum1 geloescht und das Attribut GueltigerEintrag wird obligatorisch.
      GueltigerEintrag: XMLDate;
      Datum1: XMLDate;
    UNIQUE NBIdent, Identifikator;
    END HFP1Nachfuehrung;

    CLASS HFP1 =
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Nummer : MANDATORY NummerTyp;
      Geometrie: MANDATORY LKoord;
      HoeheGeom : MANDATORY Hoehe;
      LageGen: Genauigkeit;
      LageZuv: Zuverlaessigkeit;
      HoeheGen : MANDATORY Genauigkeit;
      HoeheZuv : MANDATORY Zuverlaessigkeit;
    UNIQUE NBIdent, Nummer;
    UNIQUE Geometrie;
    END HFP1;

    ASSOCIATION HFP1Bez =
      Entstehung -- {1} HFP1Nachfuehrung;
      Von_Punkte -- {0..*} HFP1;
    END HFP1Bez;

    CLASS HFP1Pos =
      Pos: MANDATORY LKoord; !! Beschriftet Nummer :
      Ori: Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Left
      VAli: VALIGNMENT; !!  VAli undefiniert = Bottom
    END HFP1Pos;

    ASSOCIATION HFP1PosBez =
      HFP1Pos_von -<#> {1} HFP1;
