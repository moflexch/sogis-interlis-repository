  TOPIC Bodenbedeckung =

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    DOMAIN

      BBArt = (
        Gebaeude,
        befestigt (
          Strasse_Weg,
          Trottoir,
          Verkehrsinsel,
          Bahn,
          Flugplatz,
          Wasserbecken,
          uebrige_befestigte:FINAL),
        humusiert (
          Acker_Wiese_Weide,
          Intensivkultur (
            Reben,
            uebrige_Intensivkultur:FINAL),
          Gartenanlage,
          Hoch_Flachmoor,
          uebrige_humusierte:FINAL),
        Gewaesser (
          stehendes,
          fliessendes,
          Schilfguertel:FINAL),
        bestockt (
          geschlossener_Wald,
          Wytweide (  !! Wytweide = bestockte Weide, Erklaerungen Kap. 3.4
            Wytweide_dicht,
            Wytweide_offen:FINAL),
          uebrige_bestockte:FINAL),
        vegetationslos (
          Fels,
          Gletscher_Firn,
          Geroell_Sand,
          Abbau_Deponie,
          uebrige_vegetationslose:FINAL):FINAL);


    CLASS BBNachfuehrung=
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY NummerTyp;
      Beschreibung: MANDATORY NameTyp;
      Perimeter: SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
         WITHOUT OVERLAPS > 0.050;
      Gueltigkeit: MANDATORY Status;
      !! Fuer zukuenftige Nachfuehrungen ist das Attribut GueltigerEintrag zu
      !! erfassen. Datum1 betrifft die alten Nachfuehrungen und wird nicht mehr
      !! angewendet. In der naechsten Revision des Datenmodells wird das Attribut
      !! Datum1 geloescht und das Attribut GueltigerEintrag wird obligatorisch.
      GueltigerEintrag: XMLDate;
      Datum1: XMLDate;
    UNIQUE NBIdent, Identifikator;
    END BBNachfuehrung;

    CLASS ProjBoFlaeche =
        Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
          WITHOUT OVERLAPS > 0.050;
        Qualitaet: MANDATORY Qualitaetsstandard;
        Art: MANDATORY BBArt;
    END ProjBoFlaeche;

    ASSOCIATION ProjBoFlaecheBez =
        Entstehung -- {1} BBNachfuehrung;
        Von_ProjBoFlaeche -- {0..*} ProjBoFlaeche;
      MANDATORY CONSTRAINT Entstehung->Gueltigkeit == #projektiert;
    END ProjBoFlaecheBez;

    CLASS ProjGebaeudenummer =
        Nummer: MANDATORY NummerTyp;  !! Vergabe durch Kanton (bzw. Gemeinde)
        !! GWR_EGID falls die Definition der Gebaeude mit jener des
        !! BFS uebereinstimmt
        GWR_EGID: 1 .. 999999999;
    END ProjGebaeudenummer;

    ASSOCIATION ProjGebaeudenummerBez =
        ProjGebaeudenummer_von -<#> {1} ProjBoFlaeche;
        Nummer -- {0..*} ProjGebaeudenummer;
      MANDATORY CONSTRAINT ProjGebaeudenummer_von->Art == #Gebaeude;
    END ProjGebaeudenummerBez;

    CLASS ProjGebaeudenummerPos =
      Pos: MANDATORY LKoord; !! Beschriftet Nummer :
      Ori: MANDATORY Rotation;
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = Half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
    END ProjGebaeudenummerPos;

    ASSOCIATION ProjGebaeudenummerPosBez =
        ProjGebaeudenummerPos_von -<#> {1} ProjGebaeudenummer;
        Pos -- {0..*} ProjGebaeudenummerPos;
    END ProjGebaeudenummerPosBez;

    CLASS ProjObjektname =
        Name: MANDATORY NameTyp;
    END ProjObjektname;

    ASSOCIATION ProjObjektnameBez =
        ProjObjektname_von -<#> {1} ProjBoFlaeche;
        Name -- {0..*} ProjObjektname;
    END ProjObjektnameBez;

    CLASS ProjObjektnamePos =
      Pos: MANDATORY LKoord; !! Beschriftet Name :
      Ori: MANDATORY Rotation;
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = Half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
    END ProjObjektnamePos;

    ASSOCIATION ProjObjektnamePosBez =
        ProjObjektnamePos_von -<#> {1} ProjObjektname;
        Pos -- {0..*} ProjObjektnamePos;
    END ProjObjektnamePosBez;

    CLASS ProjBoFlaecheSymbol =
      !! siehe auch Bemerkungen zu BoFlaecheSymbol
        Pos: MANDATORY LKoord;
        Ori: Rotation; !! undefiniert = 0.0
    END ProjBoFlaecheSymbol;

    ASSOCIATION ProjBoFlaecheSymbolBez =
        ProjBoFlaecheSymbol_von -<#> {1} ProjBoFlaeche;
        Symbol -- {0..*} ProjBoFlaecheSymbol;
      MANDATORY CONSTRAINT CoordIn(Symbol->Pos, ProjBoFlaecheSymbol_von->Geometrie);
    END ProjBoFlaecheSymbolBez;

    CLASS BoFlaeche =
        Geometrie: MANDATORY AREA WITH (STRAIGHTS, ARCS) VERTEX LKoord
          WITHOUT OVERLAPS > 0.050;
        Qualitaet: MANDATORY Qualitaetsstandard;
        Art: MANDATORY BBArt;
    END BoFlaeche;

    ASSOCIATION BoFlaecheBez =
        Entstehung -- {1} BBNachfuehrung;
        Von_BoFlaeche -- {0..*} BoFlaeche;
      MANDATORY CONSTRAINT Entstehung->Gueltigkeit == #gueltig;
    END BoFlaecheBez;

    CLASS Gebaeudenummer =
        Nummer: MANDATORY NummerTyp;
        !! GWR_EGID falls die Definition der Gebaeude mit jener des
        !! BFS uebereinstimmt
        GWR_EGID: 1 .. 999999999;
    END Gebaeudenummer;

    ASSOCIATION GebaeudenummerBez =
        Gebaeudenummer_von -<#> {1} BoFlaeche;
        Nummer -- {0..*} Gebaeudenummer;
      MANDATORY CONSTRAINT Gebaeudenummer_von->Art == #Gebaeude;
    END GebaeudenummerBez;

    CLASS GebaeudenummerPos=
      Pos: MANDATORY LKoord; !! Beschriftet Nummer :
      Ori: MANDATORY Rotation;
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = Half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
    END GebaeudenummerPos;

    ASSOCIATION GebaeudenummerPosBez =
        GebaeudenummerPos_von -<#> {1} Gebaeudenummer;
        Pos -- {0..*} GebaeudenummerPos;
    END GebaeudenummerPosBez;

    CLASS Objektname =
        Name: MANDATORY NameTyp;
    END Objektname;

    ASSOCIATION ObjektnameBez =
        Objektname_von -<#> {1} BoFlaeche;
        Name -- {0..*} Objektname;
    END ObjektnameBez;

    CLASS ObjektnamePos =
      Pos: MANDATORY LKoord; !! Beschriftet Name :
      Ori: MANDATORY Rotation;
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = Half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
    END ObjektnamePos;

    ASSOCIATION ObjektnamePosBez =
        ObjektnamePos_von -<#> {1} Objektname;
        Pos -- {0..*} ObjektnamePos;
    END ObjektnamePosBez;

    !! Im Plan fuer das Grundbuch werden Bodenbedeckungsflaechen
    !! entweder gerastert oder mit Symbolen gefuellt. Dabei sind in
    !! Abhaengigkeit von Art nur folgende Symbole sinnvoll:
    !! befestigt.Wasserbecken, Reben, Hoch_Flachmoor
    !! (Symbol Moor), Gewaesser.stehendes (Symbol Wasserbecken),
    !! Gewaesser.fliessendes (Symbol Fliessrichtung),
    !! Gewaesser.Schilfguertel (Symbol Schilfguertel).
    CLASS BoFlaecheSymbol =
        Pos: MANDATORY LKoord;
        Ori: Rotation; !! undefiniert = 0.0
    END BoFlaecheSymbol;

    ASSOCIATION BoFlaecheSymbolBez =
        BoFlaecheSymbol_von -<#> {1} BoFlaeche;
        Symbol -- {0..*} BoFlaecheSymbol;
      MANDATORY CONSTRAINT CoordIn(Symbol->Pos, BoFlaecheSymbol_von->Geometrie);
    END BoFlaecheSymbolBez;

    CLASS Einzelpunkt =
      !! Identifikator nicht zugleich LFP1, LFP2, LFP3, Grenzpunkt oder Hoheitsgrenzpunkt
      Identifikator: NummerTyp;
      Geometrie: MANDATORY LKoord;
      LageGen: MANDATORY Genauigkeit;
      LageZuv: MANDATORY Zuverlaessigkeit;
      ExaktDefiniert: MANDATORY BOOLEAN;  !! Toleranzvorschriften gemäss TVAV beachten
    UNIQUE Geometrie;
    END Einzelpunkt;

    ASSOCIATION EinzelpunktBez =
        Entstehung -- {0..1} BBNachfuehrung;
        Von_Einzelpunkt -- {0..*} Einzelpunkt;
    END EinzelpunktBez;

    CLASS EinzelpunktPos =
      Pos: MANDATORY LKoord; !! Beschriftet Identifikator :
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Left
      VAli: VALIGNMENT; !!  VAli undefiniert = Bottom
    END EinzelpunktPos;

    ASSOCIATION EinzelpunktPosBez =
        EinzelpunktPos_von -<#> {1} Einzelpunkt;
        Pos -- {0..1} EinzelpunktPos;
    END EinzelpunktPosBez;

  END Bodenbedeckung;



