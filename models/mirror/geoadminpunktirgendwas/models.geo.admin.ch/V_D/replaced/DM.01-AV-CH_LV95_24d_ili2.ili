  TOPIC Liegenschaften =

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    DOMAIN

      Grundstuecksart = (
        Liegenschaft,
        SelbstRecht (
          Baurecht,
          Quellenrecht,
          Konzessionsrecht,
          weitere:FINAL),
        Bergwerk:FINAL);

      Gueltigkeit = (
        rechtskraeftig,
        streitig:FINAL);

      Vollstaendigkeit = (
          Vollstaendig,
          unvollstaendig:FINAL);

    STRUCTURE LinienGrenze =
        Linienart : (
          !! undefiniert bedeutet vollstaendig
          streitig,
          unvollstaendig:FINAL);
    END LinienGrenze;

    CLASS LSNachfuehrung=
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY NummerTyp;
      Beschreibung: MANDATORY NameTyp;
      Perimeter: SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
         WITHOUT OVERLAPS > 0.050;
      Gueltigkeit: MANDATORY Status;
      !! Fuer zukuenftige Nachfuehrungen sind die Attribute GueltigerEintrag und
      !! GBEintrag zu erfassen. Datum1 und Datum2 betreffen die alten
      !! Nachfuehrungen und werden nicht mehr angewendet. In der naechsten Revision
      !! des Datenmodells werden die Attribute Datum1 und Datum2 geloescht und die
      !! Attribute GueltigerEintrag und GBEintrag werden obligatorisch.
      GueltigerEintrag: XMLDate;
      GBEintrag: XMLDate;
      Datum1: XMLDate;
      Datum2: XMLDate;  !! Vergabe durch Kanton
    UNIQUE NBIdent, Identifikator;
    END LSNachfuehrung;

    !! Umfasst alle Grenzpunkte einer Liegenschaft, ausgenommen Stuetzpunkte
    !! der Liegenschaftsgrenze, die in Hoheitsgrenzpunkt und/oder LageFixpunkt
    !! enthalten sind.
    !! Siehe auch Bemerkungen zu Hoheitsgrenzpunkt (Topic Gemeindegrenzen).
   CLASS Grenzpunkt =
      Identifikator: NummerTyp;
      Geometrie: MANDATORY LKoord;
      LageGen: MANDATORY Genauigkeit;
      LageZuv: MANDATORY Zuverlaessigkeit;
      Punktzeichen: MANDATORY Versicherungsart;
      ExaktDefiniert: MANDATORY BOOLEAN;  !! Toleranzvorschriften gemäss TVAV beachten
      !! Wenn ein "huebscher" alter Hoheitsgrenzstein heruntergestuft wurde
      !! und lediglich die Funktion eines Grenzpunkts hat (siehe auch
      !! Erlaeuterungen Kap. 3.8)
      HoheitsgrenzsteinAlt: MANDATORY BOOLEAN; !! Materialangabe
    UNIQUE Geometrie;
    END Grenzpunkt;

    ASSOCIATION GrenzpunktBez =
        Entstehung -- {1} LSNachfuehrung;
        Von_Grenzpunkt -- {0..*} Grenzpunkt;
    END GrenzpunktBez;

    CLASS GrenzpunktPos =
      Pos: MANDATORY LKoord; !! Beschriftet Identifikator
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Left
      VAli: VALIGNMENT; !!  VAli undefiniert = Bottom
    END GrenzpunktPos;

    ASSOCIATION GrenzpunktPosBez =
        GrenzpunktPos_von -<#> {1} Grenzpunkt;
        Pos -- {0..1} GrenzpunktPos;
    END GrenzpunktPosBez;

    CLASS GrenzpunktSymbol =
      Ori: Rotation; !! undefiniert = 0.0
    END GrenzpunktSymbol;

    ASSOCIATION GrenzpunktSymbolBez =
        GrenzpunktSymbol_von -<#> {1} Grenzpunkt;
        Symbol -- {0..1} GrenzpunktSymbol;
    END GrenzpunktSymbolBez;

    CLASS ProjGrundstueck =
        NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
        Nummer: MANDATORY NummerTyp;
        !! Elektronisches Grundstueckinformationssystem
        EGRIS_EGRID: TEXT*14;
        !! abgeleitetes Attribut: muss streitig sein, falls ProjLiegenschaft,
        !! ProjSelbstRecht oder ProjBergwerk streitig;
        Gueltigkeit: MANDATORY Gueltigkeit;
        !! unvollstaendig, falls z.B. das ProjGrundstueck
        !! teilweise ausserhalb des Perimeters liegt.
        Vollstaendigkeit: MANDATORY Vollstaendigkeit;
        Art: MANDATORY Grundstuecksart;
       !! GesamteFlaechenmass wird nur benutzt, falls TeilGrundstueke existieren.
       !! Das heisst mehrere Objekte ProjLiegenschaft, ProjSelbstRecht
       !! oder ProjBergwerk werden zu einem Objekt ProjGrundstueck.
       GesamteFlaechenmass: 1 .. 999999999 [Units.m2];
    UNIQUE NBIdent, Nummer;
    END ProjGrundstueck;

    ASSOCIATION ProjGrundstueckBez =
        Entstehung -- {1} LSNachfuehrung;
        Von_ProjGrundstueck -- {0..*} ProjGrundstueck;
    UNIQUE Entstehung, Von_ProjGrundstueck->NBIdent, Von_ProjGrundstueck->Nummer;
    MANDATORY CONSTRAINT Entstehung->Gueltigkeit == #projektiert;
    END ProjGrundstueckBez;

    CLASS ProjGrundstueckPos =
      Pos: MANDATORY LKoord; !! Beschriftet Nummer
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = Half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
      Hilfslinie: POLYLINE WITH (STRAIGHTS) VERTEX LKoord;
      !! Hinweisstriche fuer Grundstuecknummer
    END ProjGrundstueckPos;

    ASSOCIATION ProjGrundstueckPosBez =
        ProjGrundstueckPos_von -<#> {1} ProjGrundstueck;
        Pos -- {1..*} ProjGrundstueckPos;
    END ProjGrundstueckPosBez;

    CLASS ProjLiegenschaft =
      !! Geometrie nur LFP1, LFP2, LFP3, Grenzpunkt oder Hoheitsgrenzpunkt
      NummerTeilGrundstueck: TEXT*12;
      Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.050 LINE ATTRIBUTES LinienGrenze;
      Flaechenmass: MANDATORY 1 .. 999999999 [Units.m2];
    END ProjLiegenschaft;

    ASSOCIATION ProjLiegenschaftGrundstueckBez =
        ProjLiegenschaft_von -<#> {1} ProjGrundstueck;
        Liegenschaft -- {0..*} ProjLiegenschaft;
      MANDATORY CONSTRAINT ProjLiegenschaft_von->Art == #Liegenschaft;
    END ProjLiegenschaftGrundstueckBez;

    !! Falls bei SelbstRecht keine Flaeche vorhanden ist, existiert hier
    !! auch kein Objekt. Eine Beschriftung und Lokalisierung in GrundstueckPos
    !! ist trotzdem moeglich.
    CLASS ProjSelbstRecht =
       !! Geometrie nur LFP1, LFP2, LFP3, Grenzpunkt oder Hoheitsgrenzpunkt
      NummerTeilGrundstueck: TEXT*12;
      Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.050 LINE ATTRIBUTES LinienGrenze;
      Flaechenmass: MANDATORY 1 .. 999999999 [Units.m2];
    END ProjSelbstRecht;

    ASSOCIATION ProjSelbstRechtBez =
        ProjSelbstRecht_von -<#> {1} ProjGrundstueck;
        SelbstRecht-- {0..*} ProjSelbstRecht;
      MANDATORY CONSTRAINT (ProjSelbstRecht_von->Art == #SelbstRecht.Baurecht) OR
                           (ProjSelbstRecht_von->Art == #SelbstRecht.Quellenrecht) OR
                           (ProjSelbstRecht_von->Art == #SelbstRecht.Konzessionsrecht) OR
                           (ProjSelbstRecht_von->Art == #SelbstRecht.weitere);
    END ProjSelbstRechtBez;

    !! Falls bei Bergwerk keine Flaeche vorhanden ist, existiert hier
    !! auch kein Objekt. Eine Beschriftung und Lokalisierung in GrundstueckPos
    !! ist trotzdem moeglich.
    CLASS ProjBergwerk =
      !! Geometrie nur LFP1, LFP2, LFP3, Grenzpunkt oder Hoheitsgrenzpunkt
      NummerTeilGrundstueck: TEXT*12;
      Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.050 LINE ATTRIBUTES LinienGrenze;
      Flaechenmass: MANDATORY 1 .. 999999999 [Units.m2];
    END ProjBergwerk;

    ASSOCIATION ProjBergwerkBez =
        ProjBergwerk_von -<#> {1} ProjGrundstueck;
        Bergwerk -- {0..*} ProjBergwerk;
      MANDATORY CONSTRAINT ProjBergwerk_von->Art == #Bergwerk;
    END ProjBergwerkBez;

    CLASS Grundstueck =
        NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
        Nummer: MANDATORY NummerTyp;
        !! Elektronisches Grundstueckinformationssystem
        EGRIS_EGRID: TEXT*14;
        !! abgeleitetes Attribut: muss streitig sein, falls ProjLiegenschaft,
        !! ProjSelbstRecht oder ProjBergwerk streitig;
        Gueltigkeit: MANDATORY Gueltigkeit;
        !! unvollstaendig, falls z.B. das ProjGrundstueck
        !! teilweise ausserhalb des Perimeters liegt.
        Vollstaendigkeit: MANDATORY Vollstaendigkeit;
        Art: MANDATORY Grundstuecksart;
       !! GesamteFlaechenmass wird nur benutzt, falls TeilGrundstueke existieren.
       !! Das heisst mehrere Objekte ProjLiegenschaft, ProjSelbstRecht
       !! oder ProjBergwerk werden zu einem Objekt ProjGrundstueck.
       GesamteFlaechenmass: 1 .. 999999999 [Units.m2];
    UNIQUE NBIdent, Nummer;
    END Grundstueck;

    ASSOCIATION GrundstueckBez =
        Entstehung -- {1} LSNachfuehrung;
        Von_Grundstueck -- {0..*} Grundstueck;
      MANDATORY CONSTRAINT Entstehung->Gueltigkeit == #gueltig;
    END GrundstueckBez;

    CLASS GrundstueckPos =
      Pos: MANDATORY LKoord; !! Beschriftet Nummer :
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = Half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
      Hilfslinie: POLYLINE WITH (STRAIGHTS) VERTEX LKoord; !! Hinweisstriche fuer Grundstuecksnummer
    END GrundstueckPos;

    ASSOCIATION GrundstueckPosBez =
        GrundstueckPos_von -<#> {1} Grundstueck;
        Pos -- {1..*} GrundstueckPos;
    END GrundstueckPosBez;

    CLASS Liegenschaft =
      !! Geometrie nur LFP1, LFP2, LFP3, Grenzpunkt oder Hoheitsgrenzpunkt
      NummerTeilGrundstueck: TEXT*12;
      Geometrie: MANDATORY AREA WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.050 LINE ATTRIBUTES LinienGrenze;
      Flaechenmass: MANDATORY 1 .. 999999999 [Units.m2];
    END Liegenschaft;

    ASSOCIATION LiegenschaftGrundstueckBez =
        Liegenschaft_von -<#> {1} Grundstueck;
        Liegenschaefte -- {0..*} Liegenschaft;
      MANDATORY CONSTRAINT Liegenschaft_von->Art == #Liegenschaft;
    END LiegenschaftGrundstueckBez;

    !! Falls bei SelbstRecht keine Flaeche vorhanden ist, existiert hier
    !! auch kein Objekt. Eine Beschriftung und Lokalisierung in GrundstueckPos
    !! ist trotzdem moeglich.
    CLASS SelbstRecht =
      !! Geometrie nur LFP1, LFP2, LFP3, Grenzpunkt oder Hoheitsgrenzpunkt
      NummerTeilGrundstueck: TEXT*12;
      Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.050 LINE ATTRIBUTES LinienGrenze;
      Flaechenmass: MANDATORY 1 .. 999999999 [Units.m2];
    END SelbstRecht;

    ASSOCIATION SelbstRechtBez =
      SelbstRecht_von -<#> {1} Grundstueck;
      SelbstRechte-- {0..*} SelbstRecht;
      MANDATORY CONSTRAINT (SelbstRecht_von->Art == #SelbstRecht.Baurecht) OR
                           (SelbstRecht_von->Art == #SelbstRecht.Quellenrecht) OR
                           (SelbstRecht_von->Art == #SelbstRecht.Konzessionsrecht) OR
                           (SelbstRecht_von->Art == #SelbstRecht.weitere);
    END SelbstRechtBez;

    !! Falls bei Bergwerk keine Flaeche vorhanden ist, existiert hier
    !! auch kein Objekt. Eine Beschriftung und Lokalisierung in GrundstueckPos
    !! ist trotzdem moeglich.
    CLASS Bergwerk =
      !! Geometrie nur LFP1, LFP2, LFP3, Grenzpunkt oder Hoheitsgrenzpunkt
      NummerTeilGrundstueck: TEXT*12;
      Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.050 LINE ATTRIBUTES LinienGrenze;
      Flaechenmass: MANDATORY 1 .. 999999999 [Units.m2];
    END Bergwerk;

    ASSOCIATION BergwerkBez =
        Bergwerk_von -<#> {1} Grundstueck;
        Bergwerke -- {0..*} Bergwerk;
      MANDATORY CONSTRAINT Bergwerk_von->Art == #Bergwerk;
    END BergwerkBez;

  END Liegenschaften;



  TOPIC Rohrleitungen =  !! gemaess Bundesgesetz ueber Rohrleitungsanlagen zur Befoerderung
                         !! fluessiger oder gasfoermiger Brenn- oder Treibstoffe

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    DOMAIN

      Medium = (
        Oel,
        Gas,
        weitere:FINAL);

    STRUCTURE FlaecheElementGrenze =
        !! Linienart-Werte sollen grundsaetzlich als undefiniert
        !! ausgegeben werden, ausgenommen Wert sichtbar.;
        Linienart : (
            sichtbar:FINAL);
    END FlaecheElementGrenze;


    CLASS RLNachfuehrung =
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY NummerTyp;
      Beschreibung: MANDATORY NameTyp;
      Perimeter: SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
         WITHOUT OVERLAPS > 0.050;
      Gueltigkeit: MANDATORY Status;
      !! Fuer zukuenftige Nachfuehrungen ist das Attribut GueltigerEintrag zu
      !! erfassen. Datum1 betrifft die alten Nachfuehrungen und wird nicht mehr
      !! angewendet. In der naechsten Revision des Datenmodells wird das Attribut
      !! Datum1 geloescht und das Attribut GueltigerEintrag wird obligatorisch.
      GueltigerEintrag: XMLDate;
      Datum1: XMLDate;
    UNIQUE NBIdent, Identifikator;
    END RLNachfuehrung;

    CLASS Leitungsobjekt =
        Betreiber: MANDATORY NameTyp;
        Qualitaet: MANDATORY Qualitaetsstandard;
        Art: MANDATORY Medium;
    END Leitungsobjekt;

    ASSOCIATION LeitungsobjektBez =
        Entstehung -- {1} RLNachfuehrung;
        Von_Leitungsobjekt -- {0..*} Leitungsobjekt;
    END LeitungsobjektBez;

    CLASS LeitungsobjektPos =
      Pos: MANDATORY LKoord; !! Beschriftet Betreiber
      Ori:  MANDATORY Rotation;
      HAli: MANDATORY HALIGNMENT;
      VAli: MANDATORY VALIGNMENT;
      Groesse: Schriftgroesse;  !! undefiniert = mittel
    END LeitungsobjektPos;

    ASSOCIATION LeitungsobjektPosBez =
        LeitungsobjektPos_von -<#> {1} Leitungsobjekt;
        Pos -- {0..*} LeitungsobjektPos;
    END LeitungsobjektPosBez;

    CLASS Flaechenelement =
      Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.050 LINE ATTRIBUTES FlaecheElementGrenze;
    END Flaechenelement;

    ASSOCIATION FlaechenelementBez =
        Flaechenelement_von -<#> {1} Leitungsobjekt;
        Flaechenelemente -- {0..*} Flaechenelement;
    END FlaechenelementBez;

    CLASS Linienelement =
        Geometrie: MANDATORY POLYLINE WITH (STRAIGHTS, ARCS) VERTEX LKoord;
        !! Linienart-Werte sollen grundsaetzlich als undefiniert
        !! ausgegeben werden, ausgenommen Wert sichtbar.
        Linienart: (
          sichtbar:FINAL);
    END Linienelement;

    ASSOCIATION LinienelementBez =
        Linienelement_von -<#> {1} Leitungsobjekt;
        Linienelemente -- {0..*} Linienelement;
    END LinienelementBez;

    CLASS Punktelement =
        Geometrie: MANDATORY LKoord;
        HoeheGeom: Hoehe;
        Ori: MANDATORY Rotation;
    END Punktelement;

    ASSOCIATION PunktelementBez =
        Punktelement_von -<#> {1} Leitungsobjekt;
        Punktelemente -- {0..*} Punktelement;
    END PunktelementBez;

    CLASS Signalpunkt =
        Nummer: MANDATORY NummerTyp;
        Betreiber: MANDATORY NameTyp;
        Geometrie: MANDATORY LKoord;
        Qualitaet: MANDATORY Qualitaetsstandard;
        Art: MANDATORY Medium;
        Punktart: MANDATORY (
          Signal,
          Tafel_Stein,
          weitere:FINAL);
    END Signalpunkt;

    ASSOCIATION SignalpunktBez =
        Entstehung -- {1} RLNachfuehrung;
        Von_Signalpunkt -- {0..*} Signalpunkt;
    END SignalpunktBez;

    CLASS SignalpunktPos =
      Pos: MANDATORY LKoord; !! Beschriftet Nummer
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Left
      VAli: VALIGNMENT; !!  VAli undefiniert = Bottom
    END SignalpunktPos;

    ASSOCIATION SignalpunktPosBez =
        SignalpunktPos_von -<#> {1} Signalpunkt;
        Pos -- {0..1} SignalpunktPos;
    END SignalpunktPosBez;

    CLASS Einzelpunkt =
      Identifikator: NummerTyp;
      Geometrie: MANDATORY LKoord;
      LageGen: MANDATORY Genauigkeit;
      LageZuv: MANDATORY Zuverlaessigkeit;
      ExaktDefiniert: MANDATORY BOOLEAN;  !! Toleranzvorschriften gemäss TVAV beachten
    UNIQUE Geometrie;
    END Einzelpunkt;

    ASSOCIATION EinzelpunktBez =
        Entstehung -- {0..1} RLNachfuehrung;
        Von_Einzelpunkt -- {0..*} Einzelpunkt;
    END EinzelpunktBez;

    CLASS EinzelpunktPos =
      Pos: MANDATORY LKoord; !! Beschriftet Identifikator
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Left
      VAli: VALIGNMENT; !!  VAli undefiniert = Bottom
    END EinzelpunktPos;

    ASSOCIATION EinzelpunktPosBez =
        EinzelpunktPos_von -<#> {1} Einzelpunkt;
        Pos -- {0..1} EinzelpunktPos;
    END EinzelpunktPosBez;

  END Rohrleitungen;



  TOPIC Nummerierungsbereiche =

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    DOMAIN

      Kantonskuerzel (FINAL) = (  !! BFS-Ordnung, ergaenzt mit FL und CH
        ZH, BE, LU, UR, SZ, OW, NW, GL, ZG, FR, SO, BS, BL, SH,
        AR, AI, SG, GR, AG, TG, TI, VD, VS, NE, GE, JU, FL, CH);

    !! Der hier definierte Benutzerschluessel und die zugeordnete
    !! Flaeche koennen einem Gemeindegebiet entsprechen, einem Teil
    !! oder einer Aggregation mehrerer Gemeindegebiete; dazu
    !! kommen evtl. der Kanton und/oder die Schweiz (bzw. FL)
    !! als Ganzes (siehe Dokument Erlaeuterungen)
    CLASS Nummerierungsbereich =
      Kt: MANDATORY Kantonskuerzel;    !! Eindeutig ueber die Schweiz (inkl. FL)
      NBNummer: MANDATORY TEXT*10;
      TechDossier: MANDATORY NummerTyp;
      GueltigerEintrag: XMLDate;
    UNIQUE Kt, NBNummer;  !! ergibt Benutzerschluessel NBIdent
    END Nummerierungsbereich;

    !! Die Geometrien der Nummerierungsbereiche, die das Territorium gemaess der
    !! gleichen logischen Einheit zerlegen (z.B.: Perimeter der Gemeinden),
    !! muessen untereinander eine Gebietsaufteilung (AREA) bilden.
    !! Innerhalb eines Nummerierungsbereichs muss die
    !! Geometrie ueberlappungsfrei (d.h. wie AREA) sein.
    CLASS NBGeometrie =
      Geometrie: MANDATORY SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
        WITHOUT OVERLAPS > 0.050;
    END NBGeometrie;

    ASSOCIATION NBGeometrieBez =
        NBGeometrie_von -<#> {1} Nummerierungsbereich;
        Geometrie -- {1..*} NBGeometrie;
    END NBGeometrieBez;

    CLASS NummerierungsbereichPos =
      Pos: MANDATORY LKoord; !! Beschriftet NBNummer
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Center
      VAli: VALIGNMENT; !!  VAli undefiniert = Half
      Groesse: Schriftgroesse;  !! undefiniert = mittel
    END NummerierungsbereichPos;

    ASSOCIATION NummerierungsbereichPosBez =
        NummerierungsbereichPos_von -<#> {1} Nummerierungsbereich;
        Pos -- {0..*} NummerierungsbereichPos;
    END NummerierungsbereichPosBez;

  END Nummerierungsbereiche;



  TOPIC Gemeindegrenzen =

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    STRUCTURE Gemeindegrenzattribute =
        Linienart : MANDATORY (
          rechtskraeftig, !! exakte Grenze aus Amtl. Vermessung
          streitig,       !! streitige Grenze
          provisorisch,   !! definierte Grenze, aber qualitativ ungenuegend
          undefiniert:FINAL);   !! z.B. unbekannter Seeabschluss
    END Gemeindegrenzattribute;


    CLASS GEMNachfuehrung =
      NBIdent: MANDATORY NBNummer;  !! Beziehung 1-m zu Nummerierungsbereich
      Identifikator: MANDATORY NummerTyp;
      Beschreibung: MANDATORY NameTyp;
      Perimeter: SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
         WITHOUT OVERLAPS > 0.050;
      Gueltigkeit: MANDATORY Status;
      !! Fuer zukuenftige Nachfuehrungen ist das Attribut GueltigerEintrag zu
      !! erfassen. Datum1 betrifft die alten Nachfuehrungen und wird nicht mehr
      !! angewendet. In der naechsten Revision des Datenmodells wird das Attribut
      !! Datum1 geloescht und das Attribut GueltigerEintrag wird obligatorisch.
      GueltigerEintrag: XMLDate;
      Datum1: XMLDate;
    UNIQUE NBIdent, Identifikator;
    END GEMNachfuehrung;

    !! Enthaelt alle Stuetzpunkte, die in ihrer Funktion eine
    !! Hoheitsgrenze (Landesgrenze, Kantonsgrenze, Bezirksgrenze
    !! oder Gemeindegrenze) definieren, mit
    !! folgenden zusaetzlichen Regelungen:
    !! - LFP1, LFP2 und LFP3, die an einer Hoheitsgrenze beteiligt sind,
    !!   sind hier ebenfalls enthalten; bei einer Uebernahme aus
    !!   dem Topic Fixpunkte bleiben die Attribute unveraendert;
    !! - Hoheitsgrenzsteine sind spezielle Steine (siehe Erklaerungen
    !!   Kap. 3.11); sie erhalten Hoheitsgrenzstein = ja;
    CLASS Hoheitsgrenzpunkt =
      Identifikator: NummerTyp;
      Geometrie: MANDATORY LKoord;
      LageGen: MANDATORY Genauigkeit;
      LageZuv: MANDATORY Zuverlaessigkeit;
      Punktzeichen: MANDATORY Versicherungsart;
      Hoheitsgrenzstein: MANDATORY BOOLEAN;  !! Materialangabe
      ExaktDefiniert: MANDATORY BOOLEAN;  !! Toleranzvorschriften gemäss TVAV beachten
     UNIQUE Geometrie;
    END Hoheitsgrenzpunkt;

    ASSOCIATION HoheitsgrenzpunktBez =
        Entstehung -- {1} GEMNachfuehrung;
        Von_Hoheitsgrenzpunkt -- {0..*} Hoheitsgrenzpunkt;
    END HoheitsgrenzpunktBez;

    CLASS HoheitsgrenzpunktPos =
      Pos: MANDATORY LKoord; !! Beschriftet Identifikator
      Ori:  Rotation; !! undefiniert = 100.0
      HAli: HALIGNMENT; !!  HAli undefiniert = Left
      VAli: VALIGNMENT; !!  VAli undefiniert = Bottom
    END HoheitsgrenzpunktPos;

    ASSOCIATION HoheitsgrenzpunktPosBez =
        HoheitsgrenzpunktPos_von -<#> {1} Hoheitsgrenzpunkt;
        Pos -- {0..1} HoheitsgrenzpunktPos;
    END HoheitsgrenzpunktPosBez;

    CLASS HoheitsgrenzpunktSymbol =
      Ori: Rotation; !! undefiniert = 0.0
    END HoheitsgrenzpunktSymbol;

    ASSOCIATION HoheitsgrenzpunktSymbolBez =
        HoheitsgrenzpunktSymbol_von -<#> {1} Hoheitsgrenzpunkt;
        Symbol -- {0..1} HoheitsgrenzpunktSymbol;
    END HoheitsgrenzpunktSymbolBez;

    CLASS Gemeinde =
        Name: MANDATORY NameTyp;
        BFSNr: MANDATORY 1 .. 9999;
    UNIQUE BFSNr;
    END Gemeinde;

    CLASS ProjGemeindegrenze =
        Geometrie : MANDATORY POLYLINE WITH (STRAIGHTS, ARCS) VERTEX LKoord
           WITHOUT OVERLAPS > 0.050; !! Geometrie nur Hoheitsgrenzpunkt
    END ProjGemeindegrenze;

    ASSOCIATION ProjGemeindegrenzeBez =
        Entstehung -- {1} GEMNachfuehrung;
        Von_ProjGemeindegrenze -- {0..*} ProjGemeindegrenze;
      MANDATORY CONSTRAINT Entstehung->Gueltigkeit == #projektiert;
    END ProjGemeindegrenzeBez;

    ASSOCIATION ProjGemeindegrenzeGemeindeBez =
        ProjGemeindegrenze_von -- {1} Gemeinde;
        Grenzen -- {0..*} ProjGemeindegrenze;
    END ProjGemeindegrenzeGemeindeBez;

    CLASS Gemeindegrenze =  !! auch als Exklave moeglich
        Geometrie: MANDATORY AREA WITH (STRAIGHTS, ARCS) VERTEX LKoord
          WITHOUT OVERLAPS > 0.050 LINE ATTRIBUTES Gemeindegrenzattribute;
          !! Geometrie nur Hoheitsgrenzpunkt
          !! wenn Linienart = rechtskraeftig oder streitig
    END Gemeindegrenze;

    ASSOCIATION GemeindegrenzeBez =
      Entstehung -- {1} GEMNachfuehrung;
      Von_Gemeindegrenze -- {0..*} Gemeindegrenze;
      MANDATORY CONSTRAINT Entstehung->Gueltigkeit == #gueltig;
    END GemeindegrenzeBez;

    ASSOCIATION GemeindegrenzeGemeindeBez =
      Gemeindegrenze_von -- {1} Gemeinde;
      GemeindeGrenzen -- {1..*} Gemeindegrenze;
    END GemeindegrenzeGemeindeBez;

  END Gemeindegrenzen;



  TOPIC Bezirksgrenzen =

    BASKET OID AS UUIDOID;
    OID AS UUIDOID;

    CLASS Bezirksgrenzabschnitt =
      Geometrie: MANDATORY POLYLINE WITH (STRAIGHTS, ARCS) VERTEX LKoord;
      !! Geometrie nur Hoheitsgrenzpunkt
      Gueltigkeit : MANDATORY (
        rechtskraeftig, !! exakte Grenze aus Amtl. Vermessung
        streitig,       !! streitige Grenze
        provisorisch,   !! definierte Grenze, aber qualitativ ungenuegend
        undefiniert:FINAL);   !! z.B. unbekannter Seeabschluss
    END Bezirksgrenzabschnitt;

  END Bezirksgrenzen;



  TOPIC Kantonsgrenzen =

