    /** Bestimmungsschlüssel für die pflanzennutzbare Gründigkeit (pnG) in Ab-
     * hängigkeit der Wasserhaushaltsgruppe (WhG).
     * 
     * Ehemalige Tabelle wt_gefueg
     */
    CLASS WT_BGL_pflanzennutzbare_Gruendigkeit =
      /** Wasserhaushaltsgruppe (WhG)
       */
      Wasserhaushaltsgruppe : MANDATORY TEXT*1;
      /** flanzennutzbare Gründigkeit in cm
       */
      !!@ ili2db.dispName="flanzennutzbare Gründigkeit"
      flanzennutzbare_Gruendigkeit : MANDATORY 0 .. 125 [Units.cm];
    END WT_BGL_pflanzennutzbare_Gruendigkeit;

    /** Eigenschaften der Bodeneinheit. Pro Bodeneinheit gibt es mindestens eine Bodenausprägung (Hauptausprägung) und bis zu 2 Nebenausprägung. Insgesamt müssen alle Ausprägungen 100 Prozent ergeben.
     */
    !!@ ili2db.dispName=Bodenausprägung
    CLASS Bodenauspraegung =
      /** BFS-Nr der Gemeinde (Spatial Join).
       * 
       * Datenchecker: gemnr
       */
      !!@ ili2db.dispName="Gemeinde-Nr (BFS)"
      Gemeinde_Nr : MANDATORY CHAdminCodes_V1.CHMunicipalityCode;
      /** Wald: 3-stellige Nr.
       * Landwirtschaft: 4-stellige Nr.
       * 
       * Datenchecker: objnr
       */
      Objekt_Nr : MANDATORY 100 .. 9999;
      /** Handelt es sich um die Hauptsausprägung der Fläche?
       * 
       * Datenchecker: is_hauptauspraegung
       */
      !!@ ili2db.dispName="ist Hauptausprägung"
      ist_Hauptauspraegung : MANDATORY BOOLEAN;
      /** Gewichtung der Ausprägung in Prozent (0 bis 100).
       * 
       * Datenchecker: gewichtung_auspraegung
       */
      !!@ ili2db.dispName="Gewichtung der Ausprägung"
      Gewichtung_Auspraegung : MANDATORY 0 .. 100;
      Wasserhaushaltsgruppe : MANDATORY Wasserhaushaltsgruppe;
      /** Bodentyp / Bodenart: Gemäss FAL Kap. 5.1
       * 
       * Datenchecker: bodentyp
       */
      Bodentyp : MANDATORY Bodentyp;
      /** Liste von ein oder mehreren Bodenuntertypen (Eigenschaften).
       * 
       * Ein Eintrag zum Säuregrad (E) ist obligatorisch.
       * 
       * Folgend Bodenuntertypen dürfen nur einen Eintrag haben:
       * - E Säuregrad
       * - G Grundnässe
       * - I Staunässe
       * - R Fremdnässe dauernd
       */
      !!@ ili2db.mapping=ARRAY
      Bodenuntertypen : BAG {1..*} OF Bodenuntertyp_;
      /** Geländeform: Gemäss FAL Kap. 8.2, Tabelle 8.2b
       * 
       * Datenchecker: gelform
       */
      !!@ ili2db.dispName=Geländeform
      Gelaendeform : MANDATORY Gelaendeform;
      /** Beschreibung der Kürzel fehlt.
       */
      Geologie : MANDATORY TEXT*50;
      /** Skelettgehalt Oberboden. Werteliste.
       * Alter Spaltenname: fk_skelett_ob -> skelett_t
       * 
       * Unterschiedliche Behandlung von Landwirtschafts-Flächen (LW) und Waldflächen.
       * 
       * Es können 1 oder 2 Werte zugewiesen werden.
       */
      !!@ ili2db.dispName="Skelettgehalt Oberboden"
      !!@ ili2db.mapping=Array
      Skelettgehalt_Oberboden : BAG {1..2} OF Skelettgehalt_;
      /** Skelettgehalt Unterboden. Werteliste.
       * Alter Spaltenname: fk_skelett_ub -> skelett_t
       * 
       * Unterschiedliche Behandlung von Landwirtschafts-Flächen (LW) und Waldflächen.
       * 
       * Es können 1 oder 2 Werte zugewiesen werden.
       */
      !!@ ili2db.dispName="Skelettgehalt Unterboden"
      !!@ ili2db.mapping=Array
      Skelettgehalt_Unterboden : BAG {1..2} OF Skelettgehalt_;
      /** Feinerdekörnung Oberboden.
       * Alter Spaltenname: fk_koernkl_ob -> koernkl_t
       * 
       * Einteilung aufgrund der Zahlenwerte
       * in Attribut Tongehalt_Oberboden und Schluffgehalt_Oberboden:
       * 
       *                        Tongehalt_Oberboden  Schluffgehalt_Oberboden
       * 1  Sand                 0  -   <5              0  -  <15
       * 2  schluffiger Sand     0  -   <5             15  -  <50
       * 3  lehmiger Sand        5  -  <10              0  -  <50
       * 4  lehmreicher Sand    10  -  <15              0  -  <50
       * 5  sandiger Lehm       15  -  <20              0  -  <50
       * 6  Lehm                20  -  <30              0  -  <50
       * 7  toniger Lehm        30  -  <40              0  -  <50
       * 8  lehmiger Ton        40  -  <50              0  -  <50
       * 9  Ton                 50  - <100              0  -  <50
       * 10 sandiger Schluff     0  -  <10             50  -  <70
       * 11 Schluff              0  -  <10             70  - <100
       * 12 lehmiger Schluff    10  -  <30             50  -  <90
       * 13 toniger Schluff     30  -  <50             50  -  <70
       */
      !!@ ili2db.dispName="Feinerdekörnung Oberboden"
      !!@ ili2db.mapping=Array
      Feinerdekoernung_Oberboden : BAG {0..2} OF Feinerdekoernung_;
      /** Feinerdekörnung Unterboden.
       * Alter Spaltenname: fk_koernkl_ub -> koernkl_t
       * 
       * Einteilung aufgrund der Zahlenwerte
       * in Attribut Tongehalt_Unterboden und Schluffgehalt_Unterboden:
       * 
       *                        Tongehalt_Unterboden  Schluffgehalt_Unterboden
       * 1  Sand                 0  -   <5              0  -  <15
       * 2  schluffiger Sand     0  -   <5             15  -  <50
       * 3  lehmiger Sand        5  -  <10              0  -  <50
       * 4  lehmreicher Sand    10  -  <15              0  -  <50
       * 5  sandiger Lehm       15  -  <20              0  -  <50
       * 6  Lehm                20  -  <30              0  -  <50
       * 7  toniger Lehm        30  -  <40              0  -  <50
       * 8  lehmiger Ton        40  -  <50              0  -  <50
       * 9  Ton                 50  - <100              0  -  <50
       * 10 sandiger Schluff     0  -  <10             50  -  <70
       * 11 Schluff              0  -  <10             70  - <100
       * 12 lehmiger Schluff    10  -  <30             50  -  <90
       * 13 toniger Schluff     30  -  <50             50  -  <70
       */
      !!@ ili2db.dispName="Feinerdekörnung Unterboden"
      !!@ ili2db.mapping=Array
      Feinerdekoernung_Unterboden : BAG {0..2} OF Feinerdekoernung_;
      /** Tongehalt des Oberbodens in Prozent (ganze Zahlen 0 bis 100)
       */
      !!@ ili2db.dispName="Tongehalt Oberboden"
      Tongehalt_Oberboden : MANDATORY 0 .. 100 [Units.Percent];
      /** Tongehalt des Unterbodens in Prozent (ganze Zahlen 0 bis 100)
       */
      !!@ ili2db.dispName="Tongehalt Unterboden"
      Tongehalt_Unterboden : MANDATORY 0 .. 100 [Units.Percent];
      /** Schluffgehalt Oberboden in Prozent (ganze Zahlen 0 bis 100)
       */
      !!@ ili2db.dispName="Schluffgehalt Oberboden"
      Schluffgehalt_Oberboden : MANDATORY 0 .. 100 [Units.Percent];
      /** Schluffgehalt Unterboden in Prozent (ganze Zahlen 0 bis 100)
       */
      !!@ ili2db.dispName="Schluffgehalt Unterboden"
      Schluffgehalt_Unterboden : MANDATORY 0 .. 100 [Units.Percent];
      /** Karbonatgrenze: -1 bis 500
       * 
       * Plausibilitätskriterien:
       * 
       * 0 falls Kalkgehalt_Oberboden >= 3
       * -1 falls Kalkgehalt_Oberboden <= 3
       * 
       * Das heisst, falls Karbonatgrenze = -1 dann darf
       * Kalkgehalt_Oberboden 0, 1, 2, 3 sein.
       */
      Karbonatgrenze : MANDATORY -1 .. 500;
      /** Kalkgehalt Oberboden - siehe Werteliste.
       */
      !!@ ili2db.dispName="Kalkgehalt Oberboden"
      Kalkgehalt_Oberboden : MANDATORY Kalkgehalt;
      /** Kalkgehalt Unterboden - siehe Werteliste.
       */
      !!@ ili2db.dispName="Kalkgehalt Unterboden"
      Kalkgehalt_Unterboden : MANDATORY Kalkgehalt;
      /** pH-Wert des Oberbodens. Potential des Wasserstoffs.
       * 
       * Bereich: 0 (sauer), 7 (neutral), 14 (alkalisch).
       */
      !!@ ili2db.dispName="pH-Wert Oberboden"
      pH_Wert_Oberboden : MANDATORY 0.0 .. 14.0;
      /** pH-Wert des Unterbodens. Potential des Wasserstoffs.
       * 
       * Bereich: 0 (sauer), 7 (neutral), 14 (alkalisch).
       */
      !!@ ili2db.dispName="pH-Wert Unterboden"
      pH_Wert_Unterboden : MANDATORY 0.0 .. 14.0;
      /** Mächtigkeit des Ah-Horizonts in cm.
       */
      !!@ ili2db.dispName="Mächtigkeit A-Horizont"
      Maechtigkeit_Ah_Horizont : MANDATORY 0 .. 99 [Units.cm];
      /** Humusgehalt des Ah-Horizonts in Prozent. 1 Dezimalstelle.
       */
      !!@ ili2db.dispName="Humusgehalt A-Horizont"
      Humusgehalt_Ah_Horizont : MANDATORY 0.0 .. 80.0 [Units.Percent];
      /** Humusform, nur relevant bei Waldböden. Werteliste.
       */
      !!@ ili2db.dispName="Humusform (nur bei Waldboden)"
      Humusform_Waldboden : Humusform_Waldboden;
      /** Mächtigkeit des Ahh-Horizonts in cm. Nur relevant im Waldbereich.
       */
      !!@ ili2db.dispName="Mächtigkeit Ah-Horizont"
      Maechtigkeit_Ahh_Horizont : 0.0 .. 21.0 [Units.cm];
      /** Bodenpunktzahl. Maximalpunkte: 100 (beste Qualität)
       * ehemals: bodpktzahl
       */
      Bodenpunktzahl : MANDATORY 0 .. 100;
      /** pflanzennutzbare Gründigkeit in cm.
       * 
       * Ehemals Feld pflngr
       */
      !!@ ili2db.dispName="pflanzennutzbare Gründigkeit"
      pflanzennutzbare_Gruendigkeit : MANDATORY 0 .. 250 [Units.cm];
      /** Gefügeform Oberboden. Werteliste.
       */
      !!@ ili2db.dispName="Gefügeform Oberboden"
      Gefuegeform_Oberboden : MANDATORY Gefuegeform;
      /** Gefügegrösse Oberboden. Werteliste.
       */
      !!@ ili2db.dispName="Gefügegrösse Oberboden"
      Gefuegegroesse_Oberboden : MANDATORY Gefuegegroesse;
      /** Gefügeform Unterboden. Werteliste.
       */
      !!@ ili2db.dispName="Gefügeform Unterboden"
      Gefuegeform_Unterboden : MANDATORY Gefuegegroesse;
      /** Gefügegrösse Unterboden. Werteliste.
       */
      !!@ ili2db.dispName="Gefügegrösse Unterboden"
      Gefuegegroesse_Unterboden : MANDATORY Gefuegegroesse;
      Bemerkung : MTEXT*2000;
    END Bodenauspraegung;

    ASSOCIATION Bodeneinheit_Kartierer =
      Bodeneinheit -- {0..*} Bodeneinheit;
      /** Unbekannte Kartierer (alte, bestehende Daten) sollen zu Eintrag "unbekannt" gemappt werden. Somit können zumindest bei neuen Daten keine Daten mehr ohne Zuweisung zu Kartierer eingelesen werden.
       */
      Kartierer -- {1} Kartierer;
    END Bodeneinheit_Kartierer;

    !!@ ili2db.dispName="Ausprägung zu Bodeneinheit"
    ASSOCIATION Auspraegung_Bodeneinheit =
      !!@ ili2db.dispName=Ausprägung
      Auspraegung -- {1..3} Bodenauspraegung;
      Bodeneinheit -<#> {1} Bodeneinheit;
    END Auspraegung_Bodeneinheit;

  END Bodeneinheiten;

END SO_AFU_Bodeneinheiten_20230301.
