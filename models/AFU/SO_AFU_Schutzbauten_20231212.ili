      /** Gemäss MGDM
       */
      Material : MANDATORY Baumaterial_Typ;
      Erstellungsjahr : MANDATORY 1800 .. 2050 [INTERLIS.Y];
      /** Gemäss MGDM wird "in_Abklaerung" vergeben, wenn bei
