      anthropogene_Gefaehrdung : MANDATORY SO_AFU_Geotope_20190211.Anthropogene_Gefaehrdung;
      /** Lokalname der Fundstelle des Geotops
       */
      Lokalname : MANDATORY TEXT*255;
      /** Ist das Geotop durch den Kanton geschuetzt?
       */
      kant_geschuetztes_Objekt : MANDATORY BOOLEAN;
      /** eindeutige Objektnummer des Geotops
       */
      Nummer : MANDATORY TEXT*20;
      /** alte Ingeso-Nummer
       */
