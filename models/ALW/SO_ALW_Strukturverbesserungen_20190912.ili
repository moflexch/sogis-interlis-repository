  TOPIC RaeumlicheElemente =
    OID AS INTERLIS.UUIDOID;

    CLASS Dokument =
      Titel : MANDATORY TEXT*255;
      Typ : MANDATORY SO_ALW_Strukturverbesserungen_20190912.Dokumenttyp;
