      Adresse : MANDATORY MTEXT*255;
    END Werkeigentum;

    CLASS Oekologie_Punkt
    EXTENDS Raeumliches_Element =
      Typ : MANDATORY SO_ALW_Strukturverbesserungen_20190912.Oekologie_Punkte;
